						<xs:element name="ModelNumbers" minOccurs="0">
							<xs:complexType>
								<xs:choice maxOccurs="unbounded">
									<xs:element name="ModelNumber">
										<xs:annotation>
											<xs:documentation>Model Number(s) described by this instance document.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="ModelNumber">
													<xs:attribute name="qualifer" type="Qualifier" use="required"/>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="ManufacturerModelNumber">
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="ModelNumber">
													<xs:attribute name="manufacturerName" type="NonBlankString" use="required">
														<xs:annotation>
															<xs:documentation>Keyref to manufacturer</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="PartNumbers">
							<xs:complexType>
								<xs:choice maxOccurs="unbounded">
									<xs:element name="PartNumber">
										<xs:annotation>
											<xs:documentation>Part number for the item described in the instance document.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="ModelNumber">
													<xs:attribute name="qualifier" type="Qualifier" use="required"/>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="ManufacturerPartNumber">
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="ModelNumber">
													<xs:attribute name="manufacturerName" type="NonBlankString" use="required">
														<xs:annotation>
															<xs:documentation>Keyref to manufacturer</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="Manufacturers" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Manufacturer" type="ManufacturerData" maxOccurs="unbounded"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Extension" type="Extension" minOccurs="0"/>
					</xs:all>
				</xs:complexType>
				<xs:keyref name="ManufacturerPartNumberRef" refer="ManufacturerID">
					<xs:selector xpath="./PartNumbers/ManufacturerPartNumber"/>
					<xs:field xpath="@manufacturerName"/>
				</xs:keyref>
				<xs:keyref name="ManufacturerModelNumberRef" refer="ManufacturerID">
					<xs:selector xpath="./ModelNumbers/ManufacturerModelNumber"/>
					<xs:field xpath="@manufacturerName"/>
				</xs:keyref>
				<xs:unique name="ManufacturerID">
					<xs:selector xpath="./Manufacturers/Manufacturer"/>
					<xs:field xpath="@name"/>
				</xs:unique>
			</xs:element>
			<xs:element name="Extension" type="Extension" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="NonBlankString" use="required">
			<xs:annotation>
				<xs:documentation>Name of time being described</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="version" type="NonBlankString" use="required">
			<xs:annotation>
				<xs:documentation>Version of item description.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
