	<xs:complexType name="ItemDescription">
		<xs:annotation>
			<xs:documentation>Base type for any element needing a rich set of descriptors</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Description" type="xs:string">
