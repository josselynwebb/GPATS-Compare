                             security      (u | c | s | ts)        #IMPLIED
                             bundle        NMTOKEN                 #IMPLIED
                             chglvl        CDATA                   #IMPLIED
                             pagesize      (A | B)                 #IMPLIED    
                             orient        (port | land)           #IMPLIED>

<!-- Added by ADCS Team on 7/14/04. frontsl3 was added to allow conversion of SL-3 front matter. -->
<!-- NSWC added 1-30-2006 This allows warnings, cautions and notes to be placed anywhere in the front of an SL3 document.  -->
<!ELEMENT frontsl3  (idinfo | promul | chgrec | preface | contents | ftnote | brk | figure | table | warning | caution | note)+>

<!ELEMENT volume  (title?, fronttm?, body?, rear?)>
<!ATTLIST volume  tocentry      %yesorno; "0"
                  id            ID                     #IMPLIED
                  label         CDATA                  #IMPLIED
                  num           NMTOKEN                #REQUIRED
                  chglvl        CDATA                  #IMPLIED
                  security      (u | c | s | ts)       #IMPLIED>
   
<!ELEMENT foreword  ((para | paraalts | warning | caution | note | ftnote | brk | figure | 
                                         external | xelemloc | table | chart | para0)*)>                                         
<!ATTLIST foreword
           tocentry      %yesorno;              "0"
           %bodyatt;>

<!ELEMENT preface  ((para | paraalts | warning | caution | note | ftnote | brk | figure | 
                                         external | xelemloc | table | chart | para0)*)>                                         
<!ATTLIST preface  
           tocentry      %yesorno;              "0"
           %bodyatt;>           

<!ELEMENT intro  ((para | paraalts | warning | caution | note | ftnote | brk | figure | 
                                         external | xelemloc | table | chart | para0)*)>                                         
<!ATTLIST intro  
           tocentry      %yesorno;              "0"
           %bodyatt;>
<!-- The content model for the "fronttm" element optionally contains 1 or more
     graphics, so that variances in the ordering of front matter may be
     accommodated. If data other than the document number and title contained
     in the front matter must be captured, this content model must be 
     modified. --> 

<!-- NSWC added 1-30-2006 This allows warnings, cautions and notes to be placed anywhere in the front matter of a document.  -->
<!--DCL 5-17-06: as per USMC added element wrappertable-->
<!ELEMENT fronttm    (((title?, docno) | 
                       (idinfo | warnpage | warnsum | chgsheet | lep | promul | 
                        chgrec | foreword | preface | intro | contents |  
                        illuslist | tablelist | safesum | howtouse)+), 
                       (fsection | graphic | graphicalts)*)>

<!-- NSWC added 1-30-2006 This allows warnings, cautions and notes to be placed anywhere in the front of an SL4 document.  -->
<!-- AMSEC added 11-16-2007 This allows tables within frontsl4 -->
<!ELEMENT frontsl4    (idinfo | promul | chgrec | preface | contents | cages | table)+>
<!ATTLIST frontsl4 id	CDATA	#IMPLIED>

<!ELEMENT idinfo    (pubno | nsn | chgnum | revnum | titleblk | mfr |
                        contractno | docmfr | seal | distrib | notice | downgrd |
