<!-- Element Definitions -->
<!-- The elements are defined in their hierarchical structure, from top to 
     bottom, and front to back of the document. --> 
   
<!-- docmarinecorpstm modified by ESSI on 7/30/04. docmarinecorpssl3 and docmarincorpssl4 elements added. -->
<!ELEMENT docmarinecorpstm    ((button*,foreword?, 
                                  ((fronttm, body, rear?) | (volume+)),
                                  glossary?) | docmarinecorpssl3 | docmarinecorpssl4)>
<!ELEMENT EMPTY EMPTY>
<!ELEMENT marker EMPTY>
<!ATTLIST docmarinecorpstm  id            ID                      #IMPLIED
                            security      (u | c | s | ts)        "u"
                            bundle        NMTOKEN                 #IMPLIED
                            chglvl        CDATA                   #IMPLIED
                            pagesize      (A | B)                 #IMPLIED    
                            orient        (port | land)           #IMPLIED>

<!-- docmarinecorpssl3 added by ESSI on 7/30/04. -->
<!ELEMENT docmarinecorpssl3    (frontsl3, compstocklist)>
<!ATTLIST docmarinecorpssl3  id            ID                      #IMPLIED
                             security      (u | c | s | ts)        #IMPLIED
                             bundle        NMTOKEN                 #IMPLIED
                             chglvl        CDATA                   #IMPLIED
                             pagesize      (A | B)                 #IMPLIED    
                             orient        (port | land)           #IMPLIED>

<!ELEMENT docmarinecorpssl4  ( frontsl4, ((figure | table)+ | (itemidlist, itemnocrossref, nsncrossref, partnocrossref)), circuitsymcrossref?)>
