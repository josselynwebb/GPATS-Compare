<!ELEMENT docpartn    (#PCDATA)>

<!ELEMENT doctype     (#PCDATA)>

<!ELEMENT maintlvl    %text;>

<!ELEMENT prtitle    (nomen, eqpttype?, (brk | pslist | serno | modelno | nsn)*, subject?)>

