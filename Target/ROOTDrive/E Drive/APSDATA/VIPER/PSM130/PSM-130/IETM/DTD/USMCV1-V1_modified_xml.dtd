<!ELEMENT step    (para | paraalts | warning | caution | note)*>
<!ATTLIST step  %bodyatt;>

<!ELEMENT proc    (para | paraalts | warning | caution | note)*>
<!ATTLIST proc  %bodyatt;>

<!-- compstocklist and subelements added for SL-3s by ESSI on 7/30/2004. --> 
<!--NSWC 11-01-05 Attribute added to allow individual rows of data to be addressable -->
<!--NSWC 1-19-06:  Added tocentry and bodyatt to csl  -->

<!ELEMENT compstocklist  (date, modelno, csl+)>
<!ATTLIST compstocklist  
           tocentry         %yesorno;                "0"
                        %bodyatt;>
<!ELEMENT csl  (figure?, descriptitle, table)>
<!ATTLIST csl  
           tocentry         %yesorno;                "0"
                        %bodyatt;>

<!--NSWC 08-04-05: Revised to accommodate Epic. Again-->
<!--NSWC 07-27-05: Revised to accommodate Epic -->
<!--DCL 03-10-05: LIST OF COMPONENTS (or COMPONENTS STOCKLIST (See PDF page 30 of 31) -->
<!--DCL 05-24-05: Per USMC Req. Revised model so system and subsystem are in a "row" -->
<!--DCL 09-26-05: Per USMC Req. Added enditem... -->
<!ELEMENT list-of-comp    (title?, date, enditem, locsystem+)>
<!ATTLIST list-of-comp  
          tocentry         %yesorno;                "0"
                     %bodyatt;>

<!--NSWC 07-27-05: Revised to accommodate Epic -->
<!ELEMENT locsystem    (title, locsubsys)>
<!ELEMENT locsubsys    (title, locentries+)>
<!ELEMENT locentries   (list-of-comp-entry | figure)+>

<!--NSWC 11-01-05 Attribute added to allow individual rows of data to be addressable -->
<!--NSWC 2-28-06 remark changed to remarks -->
<!ELEMENT list-of-comp-entry   (itemno, nsn, refdes, modelno, part-desc, um, qty, month, remarks)>
<!ATTLIST list-of-comp-entry  %bodyatt;>

<!--NSWC 2-28-06: removed remark from this element definition because we agreed to change remark to 
                  remarks in the DTD, but the element remarks had already been defined. -->
<!--NSWC 2-28-06: Added %bodyatt; to the subsystem, month and enditem elements.  -->

<!ELEMENT subsystem    (#PCDATA)>
<!ELEMENT month        (#PCDATA)>
<!ELEMENT enditem      (#PCDATA)>

<!ATTLIST subsystem  %bodyatt;>
<!ATTLIST month  %bodyatt;>
<!ATTLIST enditem  %bodyatt;>

<!--DCL 03-10-05: Item-Material-Action (See PDF page 16 of 31) -->
<!--DCL 03-23-05: Per USMC Req. Revised according to preferred model -->
<!--DCL 05-24-05: Per USMC Req. Revised to a CALS model -->
<!--DCL 09-26-05: Per USMC Req. changed to oper-under-unusual-->
<!--NSWC 1-19-06: Added bodyatt to location, correct-action, condition and recommendation.  -->

<!ELEMENT oper-under-unusual    (table)>
<!ATTLIST oper-under-unusual  
           tocentry         %yesorno;                "0"
                             %bodyatt;>

<!ELEMENT location  (%textcontent; | para | graphic)*>
<!ATTLIST location  %bodyatt;>

<!ELEMENT correct-action  (%textcontent; | para)*>
<!ATTLIST correct-action  %bodyatt;>

<!ELEMENT item-material   (%textcontent; | para)*>
<!ATTLIST item-material   %bodyatt;>

<!ELEMENT condition    %text;>
<!ATTLIST condition  %bodyatt;>

<!ELEMENT recommendation    %text;>
<!ATTLIST recommendation  %bodyatt;>

