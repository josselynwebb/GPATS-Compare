C *******************************************************************
C * Triangle Wave Test [VOLTAGE]
C ******************************************************************* $
C $
C $
B $
 050100 CALCULATE, 'AC-VOLTS-P' = 2.5 $
        CALCULATE, 'FREQ'       = 1000 $
        CALCULATE, 'MAX-T'      = 10 $

B $
     05 OUTPUT,
        C'          TRIANGLE WAVE [VOLTAGE] Test\LF\',
        C'  --------------------------------------\LF\',
        C'  Prepare an TRIANGLE SIGNAL of 5V Peak to Peak with a reference\LF\',
	C'  voltage of 0V and a frequency of 1 kHz to the input ports of the DMM.\LF\', 
	C'  \LF\',
        C'  T - Ready to test.\LF\',
        C'  F - Skip test.\LF\'          $

     10 INPUT, INTO'READY' $
        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 050199 $
        END, IF $

C   INSTRUMENT ONLY ACCURATE TO 46%                             $
         CALCULATE, 'VER-UL' = ('AC-VOLTS-P'/1.73205) * 1.5	$
         CALCULATE, 'VER-LL' = ('AC-VOLTS-P'/1.73205) * 0.5	$

B $
    20 VERIFY, (VOLTAGE INTO 'MEASUREMENT'), TRIANGULAR WAVE SIGNAL, UL 'VER-UL'V LL 'VER-LL'V,
           VOLTAGE MAX 10 V,
           DC-OFFSET 'DC-VOLTS'  RANGE -300 V TO 300 V,
           BANDWIDTH RANGE 3 Hz TO 300kHz,
          CNX HI DMM_1-HI LO DMM_1-LO $

        OUTPUT,
        C'   TRIANGLE WAVE [VOLTAGE], Ranged, non-Triggered \LF\',
        C'  --------------------------------------	\LF\'        $

        OUTPUT, TO 'PRINTER',
        C'   TRIANGLE WAVE [VOLTAGE], Ranged, non-Triggered \LF\',
        C'  --------------------------------------	\LF\'        $
C $
        CALCULATE, 'TESTNO' = 50120$
        PERFORM, 'RESULTS' $

         IF, 'E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI' $
        END, IF $
	
C $
B $
     99 IF, 'SUB-E-E' EQ TRUE, THEN $
            GO TO, STEP 050200 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT, INTO 'MAIN-MENU' $

        IF, 'MAIN-MENU' EQ TRUE, THEN $
	    GO TO, STEP 050005 $
	ELSE $ 
            GO TO, STEP 050100 $
        END, IF $
C $
C       END OF AC [CURRENT] TESTING - LOOP BACK TO MENU $
        GO TO, STEP 001005 $
