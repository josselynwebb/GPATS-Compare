B $
     05 OUTPUT,
         C'         AC SIGNAL              \LF\',
         C' --------------------------------\LF\',
         C' Run All Tests                  1\LF\',
         C' Voltage                        2\LF\',
	 C' Av-Voltage                     3\LF\',
         C' Voltage-p                      4\LF\',
	 C' Voltage-pp                     5\LF\',
	 C' DC-Offset                      6\LF\',
	 C' Frequency                      7\LF\',
	 C' Period                         8\LF\',
	 C' Current                        9\LF\',
	 C' Av-Current                    10\LF\',
         C' Exit                           0\LF\',
	 C'\LF\',
         C' Enter a selection\LF\'$

     10	INPUT, INTO 'SUB-CHOICE' $

        OUTPUT, 
         C' Selection:','SUB-CHOICE',C'\LF\'  $
        IF, 'SUB-CHOICE' EQ 0, THEN $
          GO TO, STEP 001005 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 1, THEN $
          CALCULATE, 'SUB-E-E' = TRUE $
          GO TO, STEP 010100 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 2, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 010100 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 3, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 010200 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 4, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 010300 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 5, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 010400 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 6, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 010600 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 7, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 010700 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 8, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 010800 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 9, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 010900 $
        END, IF $
        IF, 'SUB-CHOICE' EQ 10, THEN $
          CALCULATE, 'SUB-E-E' = FALSE $
          GO TO, STEP 011000 $
        END, IF $
 
     	OUTPUT, C'Not a valid option.  Please select from menu.' $

        GO TO, STEP 010005 $
C $ 
C *******************************************************************
C * AC SIGNAL (VOLTAGE) Test
C ******************************************************************* $
C $
B $
 010100 CALCULATE, 'AC-VOLTS-P' = 2.5 		$
        CALCULATE, 'DC-VOLTS'   = 2 		$
        CALCULATE, 'FREQ'       = 1000 		$
        CALCULATE, 'MAX-T'      = 5 		$
	CALCULATE, 'SAMP-COUNT' = 1000		$
	CALCULATE, 'PERIOD'	= 0.001		$
        CALCULATE, 'VER-UL' = ('AC-VOLTS-P' / 1.41416) * 1.1 $
        CALCULATE, 'VER-LL' = ('AC-VOLTS-P' / 1.41416) * 0.9 $
        CALCULATE, 'AC-VOLTS-RMS' = 'AC-VOLTS-P' / 1.41416 $
        CALCULATE, 'REF-VOLTS'  = 2 		$
C $
     05 OUTPUT,
        C'          AC SIGNAL [VOLTAGE] Test\LF\',
        C'  --------------------------------------\LF\',
        C'  Prepare an AC SIGNAL of 5V Peak to Peak with a reference\LF\',
	C'  voltage of 0V and a frequency of 1 kHz to the input ports of the DMM.\LF\', 
	C'  \LF\',
        C'  T - Ready to test.\LF\',
        C'  F - Skip test.\LF\'          $
C $
     10 INPUT, INTO'READY' $
        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 010199 $
        END, IF $

    20 VERIFY, (VOLTAGE INTO 'MEASUREMENT'), AC SIGNAL, UL 'VER-UL'V LL 'VER-LL'V,
           VOLTAGE MAX 300 V,
           DC-OFFSET 'DC-VOLTS'  RANGE -300 V TO 300 V,
           BANDWIDTH RANGE 3 Hz TO 300kHz,
           REF-VOLT 'REF-VOLTS' RANGE -360 V TO 360 V,
          CNX HI DMM_1-HI LO DMM_1-LO $

        CALCULATE, 'TESTNO' = 10120$
        PERFORM, 'RESULTS' $

C $
        OUTPUT,
        C'   AC [Voltage], Ranged, non-Triggered\LF\',
        C'  --------------------------------------\LF\'  $

        OUTPUT, TO 'PRINTER',
        C'   AC [Voltage], Ranged, non-Triggered\LF\', 
        C'  --------------------------------------\LF\'  $
C $
C $
        IF, 'SUB-E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI' $
        END, IF $
C $
B $
     99 IF, 'SUB-E-E' EQ TRUE, THEN $
            GO TO, STEP 010200 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT, INTO 'MAIN-MENU' $

	IF, 'MAIN-MENU' EQ TRUE, THEN $
	    GO TO, STEP 010005 $
	ELSE $
            GO TO, STEP 010100 $
