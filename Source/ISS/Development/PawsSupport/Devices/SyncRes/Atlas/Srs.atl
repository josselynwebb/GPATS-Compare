        END, IF $
        IF, 'CHOICE' EQ 3, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 122200 $
        END, IF $
        IF, 'CHOICE' EQ 4, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 122300 $
        END, IF $

     15 OUTPUT,
          C'Not a valid option.  Please select from menu.' $
        GO TO, STEP 120000 $
C $
C ********************************************************************** $
C * SRS 1 SYNCHRO INDICATOR ANGLE MEASUREMENT - SINGLE SPEED OPERATION * $
C ********************************************************************** $
B $
 121200 CALCULATE, 'ANGLE'        = 300.0 $
        CALCULATE, 'ANGLE-RATE'   = 0.0 $
        CALCULATE, 'VOLTAGE'      = 11.8 $
        CALCULATE, 'FREQ'         = 400 $
        CALCULATE, 'REF-VOLT'     = 26 $

        OUTPUT,
          C'\LF\',
          C'****** OPERATOR INSTRUCTIONS ******\LF\\LF\',
          C' Make the following connections:\LF\',
          C'   1. DS1-S1 - SD1-S1 (X)\LF\',
          C'   2. DS1-S2 - SD1-S2 (Y)\LF\',
          C'   3. DS1-S3 - SD1-S3 (Z)\LF\\LF\',
          C' T - Ready to test.\LF\',
          C' F - Skip test.\LF\' $

     10 INPUT, INTO 'READY' $
        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 121299 $
        END, IF $
C $
        APPLY, SYNCHRO,
         ANGLE 'ANGLE' RANGE 0.0 DEG TO 359.99 DEG,
         VOLTAGE 'VOLTAGE' RANGE 1 V TO 90 V,
         VOLTAGE-REF 'REF-VOLT' RANGE 2.0 V TO 115.0 V,
         CNX X SRS_SIM_1_X Y SRS_SIM_1_Y Z SRS_SIM_1_Z $

        CALCULATE, 'VER-UL' = 'ANGLE' * 1.1 $
        CALCULATE, 'VER-LL' = 'ANGLE' * 0.9 $

     20 VERIFY, (ANGLE INTO 'MEASUREMENT'), SYNCHRO,
          UL 'VER-UL' DEG LL 'VER-LL' DEG,
          ANGLE MAX 359.99 DEG,
          CNX X SRS_IND_1_X Y SRS_IND_1_Y Z SRS_IND_1_Z $
C $
     25 OUTPUT,
          C'SRS 1 SYNCHRO INDICATOR [ANGLE], SINGLE SPEED,\LF\',
          C'  NON-TRIGGERED\LF\',
          C'-----------------------------------------------------\LF\' $

        OUTPUT, TO 'PRINTER',
          C'SRS 1 SYNCHRO INDICATOR [ANGLE], SINGLE SPEED,\LF\',
          C'  NON-TRIGGERED\LF\',
          C'-----------------------------------------------------\LF\' $

        REMOVE, SYNCHRO,
            CNX X SRS_SIM_1_X Y SRS_SIM_1_Y Z SRS_SIM_1_Z $

        CALCULATE, 'TESTNO' = 121200 $
        PERFORM, 'RESULTS' $

        IF, 'E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI' $
        END, IF $
C $
B $
    99  IF, 'E-E' EQ TRUE, THEN $
            GO TO, STEP 121300 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT,  INTO 'MAIN-MENU' $

        IF, 'MAIN-MENU' EQ TRUE, THEN $
            GO TO, STEP 001005 $
        ELSE $
            GO TO, STEP 121200 $
        END, IF $
C $
C *************************************************************************** $
C * SRS 1 SYNCHRO INDICATOR ANGLE-RATE MEASUREMENT - SINGLE SPEED OPERATION * $
C *************************************************************************** $
B $
 121300 CALCULATE, 'ANGLE'        = 0 $
        CALCULATE, 'ANGLE-RATE'   = 258 $
        CALCULATE, 'VOLTAGE'      = 11.8 $
        CALCULATE, 'FREQ'         = 400 $
        CALCULATE, 'REF-VOLT'     = 26 $

        OUTPUT,
          C'\LF\',
          C'****** OPERATOR INSTRUCTIONS ******\LF\\LF\',
          C' Make the following connections:\LF\',
          C'   1. DS1-S1 - SD1-S1 (X)\LF\',
          C'   2. DS1-S2 - SD1-S2 (Y)\LF\',
          C'   3. DS1-S3 - SD1-S3 (Z)\LF\\LF\',
          C' T - Ready to test.\LF\',
          C' F - Skip test.\LF\' $

        INPUT, INTO 'READY' $
        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 121399 $
        END, IF $
C $
        APPLY, SYNCHRO,
          ANGLE-RATE 'ANGLE-RATE' RANGE 0.0 DEG/SEC TO 4896.0 DEG/SEC,
          VOLTAGE 'VOLTAGE' RANGE 1 V TO 90 V,
          VOLTAGE-REF 'REF-VOLT' RANGE 2.0 V TO 115.0 V,
          CNX X SRS_SIM_1_X Y SRS_SIM_1_Y Z SRS_SIM_1_Z $

        CALCULATE, 'VER-UL' = 'ANGLE-RATE' * 1.1 $
        CALCULATE, 'VER-LL' = 'ANGLE-RATE' * 0.9 $
C $
     20 VERIFY, (ANGLE-RATE INTO 'MEASUREMENT'), SYNCHRO,
            UL 'VER-UL' DEG/SEC LL 'VER-LL' DEG/SEC,
            ANGLE-RATE MAX 1684.8 DEG/SEC,
            CNX X SRS_IND_1_X Y SRS_IND_1_Y Z SRS_IND_1_Z $
C $
     25 OUTPUT,
          C'SRS 1 SYNCHRO INDICATOR [ANGLE-RATE], SINGLE SPEED,\LF\',
          C'  NON-TRIGGERED\LF\',
          C'-----------------------------------------------------\LF\' $

        OUTPUT, TO 'PRINTER',
          C'SRS 1 SYNCHRO INDICATOR [ANGLE-RATE], SINGLE SPEED,\LF\',
          C'  NON-TRIGGERED\LF\',
          C'-----------------------------------------------------\LF\' $

        REMOVE, SYNCHRO,
           CNX X SRS_SIM_1_X Y SRS_SIM_1_Y Z SRS_SIM_1_Z $

        CALCULATE, 'TESTNO' = 121300 $
        PERFORM, 'RESULTS' $

        IF, 'E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI' $
        END, IF $
C $
B $
     99 IF, 'E-E' EQ TRUE, THEN $
            GO TO, STEP 122200 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT,  INTO 'MAIN-MENU' $

        IF, 'MAIN-MENU' EQ TRUE, THEN $
            GO TO, STEP 001005 $
        ELSE $
            GO TO, STEP 121300 $
        END, IF $
C $
C ********************************************************************* $
C * SRS 1 SYNCHRO INDICATOR ANGLE MEASUREMENT - MULTI-SPEED OPERATION * $
C ********************************************************************* $
B $
 122200 CALCULATE, 'ANGLE'       = 250.0 $
        CALCULATE, 'ANGLE-RATE'  = 0.0 $
        CALCULATE, 'VOLTAGE'     = 11.8 $
        CALCULATE, 'FREQ'        = 400 $
        CALCULATE, 'REF-VOLT'    = 26 $
        CALCULATE, 'SPEED-RATIO' = 10 $

        OUTPUT,
          C'\LF\',
          C'****** OPERATOR INSTRUCTIONS ******\LF\\LF\',
          C' Make the following connections:\LF\',
          C'   1. DS1-S1 - SD1-S1 (X)\LF\',
          C'   2. DS1-S2 - SD1-S2 (Y)\LF\',
          C'   3. DS1-S3 - SD1-S3 (Z)\LF\\LF\',
          C'   4. DS2-S1 - SD2-S1 (X-FINE)\LF\',
          C'   5. DS2-S2 - SD2-S2 (Y-FINE)\LF\',
