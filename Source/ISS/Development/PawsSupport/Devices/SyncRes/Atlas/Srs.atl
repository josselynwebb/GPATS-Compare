 000100 BEGIN, ATLAS PROGRAM 'Srs Benchmark' $
C $
C ******************************************************************** $
C * REQUIRE STATEMENTS                                               * $
C ******************************************************************** $
C $
C $
C ******************************************************************** $
C * IDENTITY STATEMENTS                                              * $
C ******************************************************************** $
C $
        IDENTIFY, EVENT 'TRIGGER' AS (VOLTAGE), DC SIGNAL,
          EQ 1.5 V,
          INCREASING,
          CNX HI SRS_1_EXT_TRIG_HI LO SRS_1_EXT_TRIG_LO $
C $
C ******************************************************************** $
C * DECLARE STATEMENTS                                               * $
C ******************************************************************** $
C $
 000300 DECLARE, VARIABLE, 'CHOICE' IS INTEGER $
        DECLARE, VARIABLE, 'E-E' IS BOOLEAN $
        DECLARE, VARIABLE, 'MI' IS BOOLEAN $
        DECLARE, VARIABLE, 'MAIN-MENU' IS BOOLEAN $
        DECLARE, VARIABLE, 'READY' IS BOOLEAN $
        DECLARE, VARIABLE, 'AMPLITUDE' IS DECIMAL $
        DECLARE, VARIABLE, 'AC-VOLTS-P' IS DECIMAL $
        DECLARE, VARIABLE, 'AC-VOLTS-RMS' IS DECIMAL $
        DECLARE, VARIABLE, 'ANGLE' IS DECIMAL $
        DECLARE, VARIABLE, 'EFF-ANGLE' IS DECIMAL $
        DECLARE, VARIABLE, 'ANGLE-RATE' IS DECIMAL $
        DECLARE, VARIABLE, 'FAIL-FLAG' IS INTEGER $
        DECLARE, VARIABLE, 'FREQ' IS DECIMAL $
        DECLARE, VARIABLE, 'MAX-T' IS DECIMAL $
        DECLARE, VARIABLE, 'REF-VOLT' IS DECIMAL $
        DECLARE, VARIABLE, 'SPEED-RATIO' IS DECIMAL $
        DECLARE, VARIABLE, 'TESTNO' IS INTEGER $
        DECLARE, VARIABLE, 'VER-UL' IS DECIMAL $
        DECLARE, VARIABLE, 'VER-LL' IS DECIMAL $
        DECLARE, VARIABLE, 'VOLTAGE' IS DECIMAL $

C *******************************************************************
C * DEFINE MESSAGE STATEMENTS
C ******************************************************************* $
 000400 DECLARE, CONSTANT, 'CONTINUE' IS
          C' Press TRUE OR FALSE to continue ' $
C $
        DECLARE, CONSTANT, 'RETURN-MENU' IS
          C'\LF\
          Enter T - to return to menu
          Enter F - to repeat test
          ' $
C $
C        DEFINE, 'RESULTS-MSG', MESSAGE,
            UL   ()
              MV ()
            LL   () $
C $
C *******************************************************************
C * DEFINE PROCEDURE STATEMENTS
C ******************************************************************* $
C $
 000500 DEFINE, 'RESULTS', PROCEDURE $
        DECLARE, VARIABLE, 'PassFail' IS STRING (1) OF CHAR $
        DECLARE, VARIABLE, 'GoNogo' IS STRING (5) OF CHAR $
        DECLARE, VARIABLE, 'NESTED' IS BOOLEAN $

        IF, GO, THEN $
            CALCULATE, 'PassFail'= C'P' $
            CALCULATE, 'GoNogo' = C'GO  ' $
        ELSE $
            CALCULATE, 'PassFail' = C'F' $
            CALCULATE, 'GoNogo' = C'NOGO' $
            CALCULATE, 'FAIL-FLAG' = 'FAIL-FLAG' + 1 $
        END, IF $

        CALCULATE, 'NESTED' = FALSE $

        IF, 'VER-UL' EQ 0, THEN $
            IF, 'VER-LL' EQ 0, THEN $
                CALCULATE, 'NESTED' = TRUE $
            END, IF $
        END, IF $

        IF, 'NESTED', THEN $
            OUTPUT,
                C'TEST        UPPER LIMIT         MEAS VALUE          LOWER LIMIT          RESULT' $
            OUTPUT,
                C'----        ------------        ------------        ------------         ------\LF\' $
            OUTPUT,
                'TESTNO', C'        ','GO', C'               ','GoNogo', C'             ',NOGO, C'               ','PassFail', C'\LF\' $
            OUTPUT, TO 'PRINTER',
                C'TEST        UPPER LIMIT         MEAS VALUE          LOWER LIMIT          RESULT' $
            OUTPUT, TO 'PRINTER',
                C'----        ------------        ------------        ------------         ------\LF\' $
            OUTPUT, TO 'PRINTER',
                'TESTNO', C'        ','GO', C'               ','GoNogo', C'             ',NOGO, C'               ','PassFail', C'\LF\' $
        ELSE $
            OUTPUT,
                C'TEST         UPPER LIMIT               MEAS VALUE              LOWER LIMIT              RESULT\LF\' $
            OUTPUT,
                C'----        ------------               ------------            ------------             ------\LF\' $
            OUTPUT,
                'TESTNO',C'  ','VER-UL',C'    ','MEASUREMENT',C'   ','VER-LL',C'   ',C'      ', 'PassFail', C'\LF\' $
            OUTPUT, TO 'PRINTER',
                C'TEST         UPPER LIMIT               MEAS VALUE              LOWER LIMIT              RESULT\LF\' $
            OUTPUT, TO 'PRINTER',
                C'----        ------------               ------------            ------------             ------\LF\' $
            OUTPUT, TO 'PRINTER',
                'TESTNO',C'  ','VER-UL',C'    ','MEASUREMENT',C'   ','VER-LL',C'   ',C'      ', 'PassFail', C'\LF\' $
        END, IF $

        END, 'RESULTS' $
C $
C *******************************************************************
C * TEST MENU
C ******************************************************************* $
C $

E001000 OUTPUT,
          C'Srs ATLAS Driver Benchmark\LF\',
          C'Date/Time:','DATE-TIME' $

        OUTPUT, TO 'PRINTER',
          C'      Srs ATLAS Driver Benchmark\LF\',
          C'Date/Time:', 'DATE-TIME', C'\LF\' $
B $
 001005 OUTPUT,
          C'  Main Test Menu\LF\',
          C'  --------------------------------\LF\',
          C'   0 Run All Tests\LF\\LF\',
          C'   1 Synchro Simulator\LF\',
          C'   2 Synchro Indicator\LF\\LF\',
          C'   3 Resolver Simulator\LF\',
          C'   4 Resolver Indicator\LF\\LF\',
          C'   5 Triggered Simulator\LF\\LF\',
          C'  99 Exit\LF\\LF\',
          C'  Enter a selection\LF\' $

        CALCULATE, 'FAIL-FLAG' = 0 $
     10 INPUT, INTO 'CHOICE' $
        OUTPUT,
            C'  Selection:', 'CHOICE', C'\LF\' $
        IF, 'CHOICE' EQ 99, THEN $
            GO TO, STEP 999900 $
        END, IF $
        IF, 'CHOICE' EQ 0, THEN $
            CALCULATE, 'E-E' = TRUE $
            GO TO, STEP 111000 $
        END, IF $
        IF, 'CHOICE' EQ 1, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 110000 $
        END, IF $
        IF, 'CHOICE' EQ 2, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 120000 $
        END, IF $
        IF, 'CHOICE' EQ 3, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 130000 $
        END, IF $
        IF, 'CHOICE' EQ 4, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 140000 $
        END, IF $
        IF, 'CHOICE' EQ 5, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 200000 $
        END, IF $

     15 OUTPUT,
          C'Not a valid option.  Please select from menu.' $
        GO TO, STEP 001005 $
C $
C ************************************ $
C * CHANNEL 1 SYNCHRO SIMULATOR MENU * $
C ************************************ $
B $
 110000 OUTPUT,
          C'  SRS Synchro Simulator Test\LF\',
          C'    Menu\LF\',
          C'  --------------------------------\LF\',
          C'   0 Run All Tests\LF\\LF\',
          C'   1 Single Speed Operation\LF\',
          C'   2 Multi-Speed Operation\LF\\LF\',
          C'  88 Main Menu\LF\',
          C'  99 Exit\LF\\LF\',
          C'  Enter a selection\LF\' $

        CALCULATE, 'FAIL-FLAG' = 0 $
     10 INPUT, INTO 'CHOICE' $
        OUTPUT,
            C'  Selection:', 'CHOICE', C'\LF\' $
        IF, 'CHOICE' EQ 88, THEN $
            GO TO, STEP 001005 $
        END, IF $
        IF, 'CHOICE' EQ 99, THEN $
            GO TO, STEP 999900 $
        END, IF $
        IF, 'CHOICE' EQ 0, THEN $
            CALCULATE, 'E-E' = TRUE $
            GO TO, STEP 111000 $
        END, IF $
        IF, 'CHOICE' EQ 1, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 111000 $
        END, IF $
        IF, 'CHOICE' EQ 2, THEN $
            CALCULATE, 'E-E' = FALSE $
            GO TO, STEP 112000 $
        END, IF $

     15 OUTPUT,
          C'Not a valid option.  Please select from menu.' $
        GO TO, STEP 110000 $
C $
C **************************************************** $
C * SRS 1 SYNCHRO SIMULATOR - SINGLE SPEED OPERATION * $
C **************************************************** $
B $
 111000 CALCULATE, 'ANGLE'              = 240 $
        CALCULATE, 'FREQ'               = 400 $
        CALCULATE, 'VOLTAGE'            = 11.8 $
        CALCULATE, 'REF-VOLT'           = 26 $

        OUTPUT,
          C'\LF\',
          C'****** OPERATOR INSTRUCTIONS ******\LF\\LF\',
          C' Setup to measure the following\LF\',
          C' voltages:\LF\',
          C'   1. X-Z (DS1-S1 - DS1-S3)\LF\',
          C'   2. Z-Y (DS1-S3 - DS1-S2)\LF\',
          C'   3. Y-X (DS1-S2 - DS1-S1)\LF\\LF\',
          C' T - Ready to test.\LF\',
          C' F - Skip test.\LF\' $

        INPUT, INTO 'READY' $
        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 111099 $
        END, IF $

        APPLY, SYNCHRO,
          ANGLE 'ANGLE' RANGE 0.0 DEG TO 359.99 DEG,
          VOLTAGE 'VOLTAGE' RANGE 1 V TO 90 V,
          FREQ 'FREQ' RANGE 47 HZ TO 800 HZ,
          VOLTAGE-REF 'REF-VOLT' RANGE 2.0 V TO 115.0 V,
          REF-SOURCE INT,
          CNX X SRS_SIM_1_X Y SRS_SIM_1_Y Z SRS_SIM_1_Z $

        CALCULATE, 'AMPLITUDE' = SIN('ANGLE' + 120) * 'VOLTAGE' $

        OUTPUT,
          C'\LF\',
          C' Verify the voltages are as\LF\',
          C' exptected:\LF\',
          C'   X-Z: ', (SIN('ANGLE')       * 'VOLTAGE'):3:1, C'Vrms\LF\',
          C'   Z-Y: ', (SIN('ANGLE' + 120) * 'VOLTAGE'):3:1, C'Vrms\LF\',
          C'   Y-X: ', (SIN('ANGLE' + 240) * 'VOLTAGE'):3:1, C'Vrms\LF\\LF\',
          C' T - Output is correct.\LF\',
          C' F - Output is incorrect.\LF\' $
C $
        INPUT, INTO 'GO' $
C $
        CALCULATE, 'VER-UL' = 0 $
        CALCULATE, 'VER-LL' = 0 $

        OUTPUT,
          C'SRS 1 SYNCHRO SIMULATOR, SINGLE SPEED, NON-TRIGGERED\LF\',
          C'-----------------------------------------------------\LF\' $

        OUTPUT, TO 'PRINTER',
          C'SRS 1 SYNCHRO SIMULATOR, SINGLE SPEED, NON-TRIGGERED\LF\',
          C'-----------------------------------------------------\LF\' $
C $
        CALCULATE, 'TESTNO' = 111000 $
        PERFORM, 'RESULTS' $

        REMOVE, SYNCHRO,
          CNX X SRS_SIM_1_X Y SRS_SIM_1_Y Z SRS_SIM_1_Z $

        IF, 'E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI' $
        END, IF $
C $
B $
     99 IF, 'E-E' EQ TRUE, THEN $
            GO TO, STEP 112000 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT, INTO 'MAIN-MENU' $

        IF, 'MAIN-MENU' EQ TRUE, THEN $
            GO TO, STEP 001005 $
        ELSE $
            GO TO, STEP 111000 $
        END, IF $
C $
C *************************************************** $
C * SRS 1 SYNCHRO SIMULATOR - MULTI-SPEED OPERATION * $
C *************************************************** $
B $
 112000 CALCULATE, 'ANGLE'       = 129 $
        CALCULATE, 'SPEED-RATIO' = 36 $
C            EFF-ANGLE = MOD(ANGLE * SPEED-RATIO, 360) $
        CALCULATE, 'EFF-ANGLE'   = 324 $
        CALCULATE, 'VOLTAGE'     = 11.8 $
        CALCULATE, 'FREQ'        = 400 $
        CALCULATE, 'REF-VOLT'    = 26 $

        OUTPUT,
          C'\LF\',
          C'****** OPERATOR INSTRUCTIONS ******\LF\\LF\',
          C' Setup to measure the following\LF\',
          C' voltages:\LF\\LF\',
          C'   1. X-Z (DS1-S1 - DS1-S3)\LF\',
          C'   2. Z-Y (DS1-S3 - DS1-S2)\LF\',
          C'   3. Y-X (DS1-S2 - DS1-S1)\LF\\LF\',
          C'   4. Fine X-Z (DS2-S1 - DS2-S3)\LF\',
          C'   5. Fine Z-Y (DS2-S3 - DS2-S2)\LF\',
          C'   6. Fine Y-X (DS2-S2 - DS2-S1)\LF\\LF\',
          C' T - Ready to test.\LF\',
          C' F - Skip test.\LF\' $

        INPUT, INTO 'READY' $
        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 112099 $
        END, IF $

     05 APPLY, SYNCHRO,
          ANGLE 'ANGLE' RANGE 0.0 DEG TO 359.99 DEG,
          VOLTAGE 'VOLTAGE' RANGE 1 V TO 90 V,
          VOLTAGE-REF 'REF-VOLT' RANGE 2.0 V TO 115.0 V,
          REF-SOURCE INT,
          FREQ 'FREQ' RANGE 47 HZ TO 800 HZ,
          SPEED-RATIO 'SPEED-RATIO' RANGE 2 TO 50,
          CNX X SRS_SIM_1_X Y SRS_SIM_1_Y Z SRS_SIM_1_Z
              X-FINE SRS_SIM_2_X Y-FINE SRS_SIM_2_Y Z-FINE SRS_SIM_2_Z $

        OUTPUT,
          C'\LF\',
          C' Verify the voltages are as\LF\',
          C' exptected:\LF\',
          C'        X-Z: ', (SIN('ANGLE')       * 'VOLTAGE'):3:1, C'Vrms\LF\',
          C'        Z-Y: ', (SIN('ANGLE' + 120) * 'VOLTAGE'):3:1, C'Vrms\LF\',
          C'        Y-X: ', (SIN('ANGLE' + 240) * 'VOLTAGE'):3:1, C'Vrms\LF\\LF\',
          C'   Fine X-Z: ', (SIN('EFF-ANGLE')       * 'VOLTAGE'):3:1, C'Vrms\LF\',
          C'   Fine Z-Y: ', (SIN('EFF-ANGLE' + 120) * 'VOLTAGE'):3:1, C'Vrms\LF\',
          C'   Fine Y-X: ', (SIN('EFF-ANGLE' + 240) * 'VOLTAGE'):3:1, C'Vrms\LF\\LF\',
          C' T - Output is correct.\LF\',
          C' F - Output is incorrect.\LF\' $
C $
        INPUT, INTO 'GO' $
C $
        CALCULATE, 'VER-UL' = 0 $
        CALCULATE, 'VER-LL' = 0 $

        OUTPUT,
          C'SRS 1/2 SYNCHRO SIMULATOR, MULTI-SPEED, NON-TRIGGERED\LF\',
          C'-----------------------------------------------------\LF\' $

        OUTPUT, TO 'PRINTER',
          C'SRS 1/2 SYNCHRO SIMULATOR, MULTI-SPEED, NON-TRIGGERED\LF\',
          C'-----------------------------------------------------\LF\' $

     10 REMOVE, SYNCHRO,
          CNX X SRS_SIM_1_X Y SRS_SIM_1_Y Z SRS_SIM_1_Z
              X-FINE SRS_SIM_2_X Y-FINE SRS_SIM_2_Y Z-FINE SRS_SIM_2_Z $

        CALCULATE, 'TESTNO' = 112000 $
        PERFORM, 'RESULTS' $

        IF, 'E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI' $
        END, IF $
C $
B $
     99 IF, 'E-E' EQ TRUE, THEN $
            GO TO, STEP 121200 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT,  INTO 'MAIN-MENU' $

        IF, 'MAIN-MENU' EQ TRUE, THEN $
            GO TO, STEP 001005 $
        ELSE $
            GO TO, STEP 112000 $
        END, IF $
C $
C ************************************ $
C * CHANNEL 1 SYNCHRO INDICATOR MENU * $
C ************************************ $
B $
 120000 OUTPUT,
          C'  SRS Synchro Indicator Test\LF\',
          C'    Menu\LF\',
          C'  --------------------------------\LF\',
          C'   0 Run All Tests\LF\\LF\',
          C'   1 Angle Measurement, Single Speed\LF\',
          C'   2 Angle-Rate Measurement, Single Speed\LF\\LF\',
          C'   3 Angle Measurement, Multi-Speed\LF\',
          C'   4 Angle-Rate Measurement, Multi-Speed\LF\\LF\',
          C'  88 Main Menu\LF\',
          C'  99 Exit\LF\\LF\',
          C'  Enter a selection\LF\' $

        CALCULATE, 'FAIL-FLAG' = 0 $
     10 INPUT, INTO 'CHOICE' $
        OUTPUT,
          C'  Selection:', 'CHOICE', C'\LF\' $
        IF, 'CHOICE' EQ 88, THEN $
            GO TO, STEP 001005 $
        END, IF $
        IF, 'CHOICE' EQ 99, THEN $
            GO TO, STEP 999900 $
        END, IF $
        IF, 'CHOICE' EQ 0, THEN $
            CALCULATE, 'E-E' = TRUE $
            GO TO, STEP 121200 $
        END, IF $
        IF, 'CHOICE' EQ 1, THEN $
