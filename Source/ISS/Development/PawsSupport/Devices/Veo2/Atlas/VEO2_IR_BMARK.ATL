C * PROCEDURE: 'TEMPERATURE MEASUREMENTS'                            * $
C ******************************************************************** $
C                                                                      $
 012000 DEFINE, 'TEMPERATURE MEASUREMENTS', PROCEDURE                  $
C                                                                      $
     05   DECLARE, VARIABLE, 'TEMP' IS DECIMAL                         $
     10   OUTPUT, 
            C'IR Source Temperature Measurements'                      $
C   ****************************************************************   $
C   * Test 0121 - Ambient Temperature Measurement                  *   $
C   ****************************************************************   $
 012100   OUTPUT,
            C'IR Source Ambient Measurement Test'                      $
     05   CALCULATE, 'ULIM' = 27.00,
                     'LLIM' =  20.00,
                     'TESTNO' = 012100                                 $
     10   SETUP, INFRARED,
            DIFFERENTIAL-TEMP -2 DEGC,
            DIFF-TEMP-ERROR 0.1 DEGC,
            MAX-TIME 120 SEC,
            CNX VIA ATMOSPHERE                                         $
     15   MEASURE, (AMBIENT-TEMP INTO 'MVAL'), INFRARED,
            AMBIENT-TEMP RANGE -10.0 DEGC TO 55.0 DEGC,
            MAX-TIME 60 SEC,
            CNX VIA ATMOSPHERE                                         $
     20   PERFORM, 'RESULTS'('TESTNO', 'MVAL', 'ULIM', 'LLIM')         $
     25   CALCULATE, 'TEMP' = 'MVAL' - 1.0                             $
     30   INPUT, GO-NOGO                                               $
C   ****************************************************************   $
C   * Test 0122 - Blackbody Temperature Measurement                *   $
C   ****************************************************************   $
 012200   OUTPUT,
            C'IR Source Blackbody Measurement Test'                    $
     05   CALCULATE, 'ULIM' = 'MVAL' - 1.0,
                     'LLIM' = 'MVAL' - 3.0,
                     'TESTNO' = 012200                                 $
     10   WAIT FOR, 10 SEC                                             $
     15   MEASURE, (BLACKBODY-TEMP INTO 'MVAL'), INFRARED,
            BLACKBODY-TEMP RANGE -10.0 DEGC TO 65.0 DEGC,
            MAX-TIME 60 SEC,
            CNX VIA ATMOSPHERE                                         $
     20   PERFORM, 'RESULTS'('TESTNO', 'MVAL', 'ULIM', 'LLIM')         $
     25   INPUT, GO-NOGO                                               $
C   ****************************************************************   $
C   * Test 0123 - Differential Temperature Measurement             *   $
C   ****************************************************************   $
 012300   OUTPUT,
            C'IR Source Differential Measurement Test'                 $
     05   CALCULATE, 'ULIM' = 5.05,
                     'LLIM' = 4.95,
                     'TESTNO' = 012300                                 $
     10   SETUP, INFRARED,
            DIFFERENTIAL-TEMP 5 DEGC,
            DIFF-TEMP-ERROR 0.03 DEGC,
            MAX-TIME 120 SEC,
            CNX VIA ATMOSPHERE                                         $
     15   MEASURE, (DIFFERENTIAL-TEMP INTO 'MVAL'), INFRARED,
            DIFFERENTIAL-TEMP RANGE -10.0 DEGC TO 10.0 DEGC,
            MAX-TIME 60 SEC,
            CNX VIA ATMOSPHERE                                         $
     20   PERFORM, 'RESULTS'('TESTNO', 'MVAL', 'ULIM', 'LLIM')         $
     25   INPUT, GO-NOGO                                               $
C   ****************************************************************   $
C   * Test 0124 - Differential Temperature Measurement             *   $
C   ****************************************************************   $
 012400   OUTPUT,
            C'IR Source Differential Measurement Test'                 $
     05   CALCULATE, 'ULIM' = -4.95,
                     'LLIM' = -5.05,
                     'TESTNO' = 012400                                 $
     10   SETUP, INFRARED,
            DIFFERENTIAL-TEMP -5 DEGC,
            DIFF-TEMP-ERROR 0.05 DEGC,
            MAX-TIME 300 SEC,
            CNX VIA ATMOSPHERE                                         $
     15   WAIT FOR, 10 SEC                                             $
     20   MEASURE, (DIFFERENTIAL-TEMP INTO 'MVAL'), INFRARED,
            DIFFERENTIAL-TEMP RANGE -10.0 DEGC TO 10.0 DEGC,
            MAX-TIME 60 SEC,
            CNX VIA ATMOSPHERE                                         $
     25   PERFORM, 'RESULTS'('TESTNO', 'MVAL', 'ULIM', 'LLIM')         $
     30   INPUT, GO-NOGO                                               $
C   ****************************************************************   $
C   * Test 0126 - Blackbody Temperature Measurement                *   $
C   ****************************************************************   $
 012500   OUTPUT,
            C'IR Source Blackbody Measurement Test'                    $
     05   CALCULATE, 'ULIM' = 25.00,
                     'LLIM' = 15.00,
                     'TESTNO' = 012600                                 $
     10   MEASURE, (BLACKBODY-TEMP INTO 'MVAL'), INFRARED,
            BLACKBODY-TEMP MAX 60.0 DEGC,
            CNX VIA ATMOSPHERE                                         $
     20   PERFORM, 'RESULTS'('TESTNO', 'MVAL', 'ULIM', 'LLIM')         $
     25   RESET, INFRARED,
            CNX VIA ATMOSPHERE                                         $
C                                                                      $
 012699 END, 'TEMPERATURE MEASUREMENTS'                                $
C                                                                      $
