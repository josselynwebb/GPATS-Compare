C ******** 2 SUPPLIES IN PARALLEL*********************$
B $
 030100 OUTPUT, 
	C'     2 SUPPLIES PARALLEL TEST
              --------------------------\LF\' $
	OUTPUT, TO 'PRINTER',
	C'     2 SUPPLIES PARALLEL TEST
              --------------------------\LF\' $
        OUTPUT,
        C'   2 SUPPLIES PARALLEL TEST - 1 and 2
          ------------------------------------------
          Connect power supply 1 and 2 outputs in 
          parallel with 15 Ohm load.  Remove ITA box and use special
          connector to do this.  Connect supply #1 
          sense lines to supply 1 output.

          T - Ready to test.
          F - Skip test. '
          $

	INPUT, INTO 'READY' $

        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 030020 $
        END, IF $
C $
	CALCULATE, 'IMPEDANCE'    = 15.0 $
	CALCULATE, 'VOLTAGE'    = 20 $
        CALCULATE, 'CURR-EXP' = ('VOLTAGE'/'IMPEDANCE') $
	CALCULATE, 'CURRENT' = 5 $
	CALCULATE, 'CURR-LMT' = 8 $
	CALCULATE, 'MAX-T' = 10 $

     20	CALCULATE, 'VOLT-UL' = ('VOLTAGE') * 1.1 $
        CALCULATE, 'VOLT-LL' = ('VOLTAGE') * 0.9 $
	CALCULATE, 'CURR-UL' = ('CURR-EXP') * 1.1 $
        CALCULATE, 'CURR-LL' = ('CURR-EXP') * 0.9 $
C $
	OUTPUT, 'VOLTAGE':5:2, C' V is applied.\LF\'  $



	APPLY, DC SIGNAL,
           VOLTAGE 'VOLTAGE' RANGE -40 V TO 40 V,
           CURRENT-LMT 'CURR-LMT' RANGE 5.0001 A TO 10 A,
           CNX HI DCPS_2MS-HI LO DCPS_2MS-LO
               SENSE-HI DCPS_2MS-SENHI SENSE-LO DCPS_2MS-SENLO $
	OUTPUT, 'CONTINUE' $
	INPUT, INTO 'READY' $
	CALCULATE, 'TESTNO' = 030120 $
C	Sense lines at only 1 of the parallel supplies is used 
C       to measure resultant voltage and current   $
        VERIFY, (VOLTAGE INTO 'VOLTAGE'), DC SIGNAL, UL 'VOLT-UL' V LL 'VOLT-LL' V,
	   VOLTAGE MAX 'VOLT-MAX' RANGE -40 V TO 40 V,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX SENSE-HI DCPS_1MS-SENHI SENSE-LO DCPS_1MS-SENLO $
	CALCULATE, 'MEASUREMENT' = 'VOLTAGE' $
	PERFORM, 'V-RESULTS' $

        MEASURE, (CURRENT INTO 'CURRENT1'), DC SIGNAL,
	   CURRENT MAX 'CURR-MAX' RANGE 0 A TO 5 A,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX SENSE-HI DCPS_1MS-SENHI SENSE-LO DCPS_1MS-SENLO $

	MEASURE, (CURRENT INTO 'CURRENT2'), DC SIGNAL,
	   CURRENT MAX 'CURR-MAX' RANGE 0 A TO 5 A,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX SENSE-HI DCPS_2MS-SENHI SENSE-LO DCPS_2MS-SENLO $
	CALCULATE, 'MEASUREMENT' = 'CURRENT1' + 'CURRENT2' $
	PERFORM, 'I-RESULTS' $

C $
	OUTPUT, C' \LF\',
          C'----------------------------------------          \LF\',
          C'The readback values are:              \LF\', 
	  C'Voltage supply 1 = ','VOLTAGE':5:2,C' V (+/-10%). \LF\',
	  C'Current (Total)  = ','MEASUREMENT':5:2,C' A (+/-10%). \LF\',
  	  C'                                                  \LF\',	
          C'T - Test Passed                                   \LF\',
          C'F - Test Failed                                   \LF\',
	  $
 
        INPUT, INTO 'READY' $
C $
        REMOVE,  DC SIGNAL,
           CNX HI DCPS_2MS-HI LO DCPS_2MS-LO $

	IF, 'E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI'$
        END, IF $


