 000100 BEGIN, ATLAS PROGRAM 'DcPs Benchmark' $
C ***************************************************************
  * Purpose: To test all modifiers and ranges as required for the
  *          WcemDcPs.dll.
  * Subset:  PAWS_85 or PAWS_89
  *
  **************************************************************$
C$
C	REQUIRE, 'PRINTER', I-O DEVICE,
        	CAPABILITY,
			LINE-LENGTH 80 CHAR $
C			PAGE-SIZE 25 LINES $

C *******************************************************************
C * DECLARE STATEMENTS
C ******************************************************************* $
C $
 000200 DECLARE, VARIABLE, 'CHOICE' IS INTEGER		$
	DECLARE, VARIABLE, 'E-E' IS BOOLEAN		$
	DECLARE, VARIABLE, 'MI' IS BOOLEAN		$
	DECLARE, VARIABLE, 'MAIN-MENU' IS BOOLEAN	$
	DECLARE, VARIABLE, 'READY' IS BOOLEAN		$
	DECLARE, VARIABLE, 'VOLTAGE' IS DECIMAL	        $
	DECLARE, VARIABLE, 'VOLT-MAX' IS DECIMAL	$
	DECLARE, VARIABLE, 'VOLT-EXP' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT' IS DECIMAL	        $
        DECLARE, VARIABLE, 'CURR-MAX' IS DECIMAL	$
	DECLARE, VARIABLE, 'CURR-EXP' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT1' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT2' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT3' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT4' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT5' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT6' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT7' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT8' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURRENT9' IS DECIMAL	$
        DECLARE, VARIABLE, 'VOLT-LMT' IS DECIMAL	$
        DECLARE, VARIABLE, 'CURR-LMT' IS DECIMAL	$
        DECLARE, VARIABLE, 'IMPEDANCE' IS DECIMAL	$
	DECLARE, VARIABLE, 'AC-VOLTS-RMS' IS DECIMAL	$
	DECLARE, VARIABLE, 'DC-VOLTS' IS DECIMAL	$
	DECLARE, VARIABLE, 'FREQ' IS DECIMAL		$
	DECLARE, VARIABLE, 'MEASUREMENT' IS DECIMAL		$
	DECLARE, VARIABLE, 'MAX-T' IS DECIMAL		$
	DECLARE, VARIABLE, 'VOLT-UL' IS DECIMAL		$
	DECLARE, VARIABLE, 'VOLT-LL' IS DECIMAL		$
        DECLARE, VARIABLE, 'CURR-UL' IS DECIMAL		$
	DECLARE, VARIABLE, 'CURR-LL' IS DECIMAL		$
        DECLARE, VARIABLE, 'VER-UL' IS DECIMAL		$
	DECLARE, VARIABLE, 'VER-LL' IS DECIMAL		$
	DECLARE, VARIABLE, 'FAIL-FLAG' IS INTEGER	$
	DECLARE, VARIABLE, 'TESTNO' IS INTEGER		$
C *******************************************************************
C * DEFINE MESSAGE STATEMENTS
C ******************************************************************* $
 000300 DECLARE, CONSTANT, 'CONTINUE' IS
         C' Press TRUE OR FALSE to continue '
          $
C $
        DECLARE, CONSTANT, 'RETURN-MENU' IS
        C'\LF\
	Enter T - to return to menu
        Enter F - to repeat test
	'
        $
C $
C        DEFINE, 'RESULTS-MSG', MESSAGE,
            UL   ()
              MV ()
            LL   () $
C $
C *******************************************************************
C * DEFINE PROCEDURE STATEMENTS
C ******************************************************************* $
C $
 000400 DEFINE, 'V-RESULTS', PROCEDURE $
        DECLARE, VARIABLE, 'PassFail' IS STRING (1) OF CHAR $
        DECLARE, VARIABLE, 'GoNogo' IS STRING (5) OF CHAR $
        DECLARE, VARIABLE, 'NESTED' IS BOOLEAN $

        IF, GO, THEN $
           CALCULATE, 'PassFail'= C'P' $
	   CALCULATE, 'GoNogo' = C'GO  ' $
         ELSE  $
	   CALCULATE, 'PassFail' = C'F' $
	   CALCULATE, 'GoNogo' = C'NOGO' $
           CALCULATE, 'FAIL-FLAG' = 'FAIL-FLAG' + 1 $
         END, IF $

         CALCULATE, 'NESTED' = FALSE $

         IF, 'VOLT-UL' EQ 0, THEN $

             IF, 'VOLT-LL' EQ 0, THEN $

                 CALCULATE, 'NESTED' = TRUE $

             END, IF $

         END, IF $

        IF, 'NESTED', THEN $		

              OUTPUT, 
		C'TEST        UPPER LIMIT         MEAS VALUE          LOWER LIMIT          RESULT'   $

              OUTPUT, 
		C'----        ------------        ------------        ------------         ------\LF\' $ 

              OUTPUT, 
		'TESTNO', C'    ','GO', C'    ','GoNogo', C'   ',NOGO, C'  ','PassFail', C'\LF\'$ 

        
              OUTPUT, TO 'PRINTER', 
		C'TEST        UPPER LIMIT         MEAS VALUE          LOWER LIMIT          RESULT'   $

              OUTPUT, TO 'PRINTER',
		C'----        ------------        ------------        ------------         ------\LF\' $ 

              OUTPUT, TO 'PRINTER',
		'TESTNO', C'    ','GO', C'    ','GoNogo', C'   ',NOGO, C'  ','PassFail', C'\LF\'$ 

  
          ELSE $

              OUTPUT, 
		C'TEST         UPPER LIMIT               MEAS VALUE              LOWER LIMIT              RESULT\LF\'   $

              OUTPUT, 
		C'----        ------------               ------------            ------------             ------\LF\'   $ 

              OUTPUT, 
	       'TESTNO',C'    ','VOLT-UL':8:4,C'                   ','MEASUREMENT':8:4,C'              ','VOLT-LL':8:4,C'                   ','PassFail', C'\LF\\LF\'$

              OUTPUT, TO 'PRINTER', 
		C'TEST         UPPER LIMIT               MEAS VALUE              LOWER LIMIT              RESULT\LF\'   $

              OUTPUT, TO 'PRINTER',
		C'----        ------------               ------------            ------------             ------\LF\'   $ 

              OUTPUT, TO 'PRINTER',
	       'TESTNO',C'    ','VOLT-UL':8:4,C'                    ','MEASUREMENT':8:4,C'             ','VOLT-LL':8:4,C'                   ', 'PassFail', C'\LF\\LF\'$
 

         END, IF $


        END, 'V-RESULTS' $

 000500 DEFINE, 'I-RESULTS', PROCEDURE $
        DECLARE, VARIABLE, 'PassFail' IS STRING (1) OF CHAR $
        DECLARE, VARIABLE, 'GoNogo' IS STRING (5) OF CHAR $
        DECLARE, VARIABLE, 'NESTED' IS BOOLEAN $

        IF, GO, THEN $
           CALCULATE, 'PassFail'= C'P' $
	   CALCULATE, 'GoNogo' = C'GO  ' $
         ELSE  $
	   CALCULATE, 'PassFail' = C'F' $
	   CALCULATE, 'GoNogo' = C'NOGO' $
           CALCULATE, 'FAIL-FLAG' = 'FAIL-FLAG' + 1 $
         END, IF $

         CALCULATE, 'NESTED' = FALSE $

         IF, 'CURR-UL' EQ 0, THEN $

             IF, 'CURR-LL' EQ 0, THEN $

                 CALCULATE, 'NESTED' = TRUE $

             END, IF $

         END, IF $

        IF, 'NESTED', THEN $		

              OUTPUT, 
		C'TEST        UPPER LIMIT         MEAS VALUE          LOWER LIMIT          RESULT'   $

              OUTPUT, 
		C'----        ------------        ------------        ------------         ------\LF\' $ 

              OUTPUT, 
		'TESTNO', C'    ','GO', C'    ','GoNogo', C'   ',NOGO, C'  ','PassFail', C'\LF\'$ 

        
              OUTPUT, TO 'PRINTER', 
		C'TEST        UPPER LIMIT         MEAS VALUE          LOWER LIMIT          RESULT'   $

              OUTPUT, TO 'PRINTER',
		C'----        ------------        ------------        ------------         ------\LF\' $ 

              OUTPUT, TO 'PRINTER',
		'TESTNO', C'    ','GO', C'    ','GoNogo', C'   ',NOGO, C'  ','PassFail', C'\LF\'$ 

  
          ELSE $

              OUTPUT, 
		C'TEST         UPPER LIMIT               MEAS VALUE              LOWER LIMIT              RESULT\LF\'   $

              OUTPUT, 
		C'----        ------------               ------------            ------------             ------\LF\'   $ 

              OUTPUT, 
	       'TESTNO',C'    ','CURR-UL':8:4,C'                   ','MEASUREMENT':8:4,C'              ','CURR-LL':8:4,C'                   ','PassFail', C'\LF\\LF\'$

              OUTPUT, TO 'PRINTER', 
		C'TEST         UPPER LIMIT               MEAS VALUE              LOWER LIMIT              RESULT\LF\'   $

              OUTPUT, TO 'PRINTER',
		C'----        ------------               ------------            ------------             ------\LF\'   $ 

              OUTPUT, TO 'PRINTER',
	       'TESTNO',C'    ','CURR-UL':8:4,C'                    ','MEASUREMENT':8:4,C'             ','CURR-LL':8:4,C'                   ', 'PassFail', C'\LF\\LF\'$
 

         END, IF $


        END, 'I-RESULTS' $
C ******************************************************************* $
C $
C *******************************************************************
C * TEST MENU
C ******************************************************************* $
C $

E001000 OUTPUT,
      C'DcPs ATLAS Driver Benchmark\LF\',
      C'Date/Time:','DATE-TIME' $
	
	OUTPUT, TO 'PRINTER', C'      DcPs ATLAS Driver Benchmark\LF\',
      C'Date/Time:', 'DATE-TIME', C'\LF\\LF\\LF\' $


c	APPLY, SHORT, FROM J1 TO J2 $
B $
 001005  OUTPUT, 
        C'           Main Menu
          --------------------------------
          Run All Tests                  1
          Single Supply DCPS 1-10 tests  2
          Master-Slave parallel tests    3
          Current Limit tests            4
	  Sequence Supplies	 	 5
          Exit                           0

          Enter a selection'
        $

        CALCULATE, 'FAIL-FLAG' = 0 $

     10 INPUT, INTO 'CHOICE' $

        OUTPUT, 
        C'Selection:', 'CHOICE', C'\LF\'  $
        IF, 'CHOICE' EQ 0, THEN $
          GO TO, STEP 999900 $
        END, IF $
        IF, 'CHOICE' EQ 1, THEN $
          CALCULATE, 'E-E' = TRUE $
          GO TO, STEP 010100 $
        END, IF $
        IF, 'CHOICE' EQ 2, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 010000 $
        END, IF $
        IF, 'CHOICE' EQ 3, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 030000 $
        END, IF $
	IF, 'CHOICE' EQ 4, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 050000 $
        END, IF $
	IF, 'CHOICE' EQ 5, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 055000 $
        END, IF $
        
	
     15 OUTPUT, 
          C' Bad Choice!  Try again '
           $

        GO TO, STEP 001005 $

C $

C *************************************************************$
C ****** TEST POWER SUPPLIES 1-10 *****************************$
C *************************************************************$

B $
 010000 CALCULATE, 'IMPEDANCE'    = 13.5 $
	CALCULATE, 'CURR-MAX'	  = 5 $
        CALCULATE, 'VOLT-MAX'	  = 40 $

	OUTPUT, C' CLOSE SWITCHES TO SWITCH FROM 13 OHM LOAD
                   TO 13.5 OHM LOAD:
			   S101-37,38
                           S101-39,40  \LF\\LF\'   $

	OUTPUT, 
        C'   DCPS_1 - DCPS_10 Test Menu
          --------------------------------
          DCPS_1                         1
          DCPS_2		         2
          DCPS_3		         3
          DCPS_4                         4
          DCPS_5		         5
          DCPS_6		         6
          DCPS_7                         7
          DCPS_8		         8
          DCPS_9		         9
          DCPS_10                        10
          Main Menu                      0

          Enter a selection\LF\'
        $

     05	CALCULATE, 'FAIL-FLAG' = 0 $

     10 INPUT, INTO 'CHOICE' $

        OUTPUT, 
        C'Selection:', 'CHOICE', C'\LF\'  $
        IF, 'CHOICE' EQ 0, THEN $
          GO TO, STEP 001005 $
        END, IF $
        IF, 'CHOICE' EQ 1, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 010100 $
        END, IF $
        IF, 'CHOICE' EQ 2, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020100 $
        END, IF $
        IF, 'CHOICE' EQ 3, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020200 $
        END, IF $
        IF, 'CHOICE' EQ 4, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020300 $
        END, IF $
        IF, 'CHOICE' EQ 5, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020400 $
        END, IF $
        IF, 'CHOICE' EQ 6, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020500 $
        END, IF $
        IF, 'CHOICE' EQ 7, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020600 $
        END, IF $
        IF, 'CHOICE' EQ 8, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020700 $
        END, IF $
        IF, 'CHOICE' EQ 9, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020800 $
        END, IF $
	IF, 'CHOICE' EQ 10, THEN $
          CALCULATE, 'E-E' = FALSE $
          GO TO, STEP 020900 $
        END, IF $

     15 OUTPUT, 
          C' Bad Choice!  Try again '
           $

        GO TO, STEP 010000 $


C *******************************************************************
C * DCPS_1 Tests
C ******************************************************************* $
	


C $
B $
 010100	OUTPUT, 
	C'     DCPS1 TESTS
              --------------\LF\' $
	OUTPUT, TO 'PRINTER',
	C'     DCPS1 TESTS
              --------------\LF\' $

	OUTPUT, 
	C'     DC SIGNAL TEST-VOLTAGE
              ------------------------\LF\' $
	OUTPUT, TO 'PRINTER',
	C'     DC SIGNAL TEST-VOLTAGE
              ------------------------\LF\' $

        
      	CALCULATE, 'IMPEDANCE'    = 13.5 $
	CALCULATE, 'VOLTAGE'    = 5 $
        CALCULATE, 'CURR-EXP' = 'VOLTAGE'/'IMPEDANCE' $
	CALCULATE, 'CURR-MAX'=5 $
        CALCULATE, 'CURR-LMT' = 4 $
	CALCULATE, 'MAX-T' = 10  $
	CALCULATE, 'VOLT-UL' = ('VOLTAGE') * 1.1 $
        CALCULATE, 'VOLT-LL' = ('VOLTAGE') * 0.9 $
	CALCULATE, 'CURR-UL' = ('CURR-EXP') * 1.1 $
        CALCULATE, 'CURR-LL' = ('CURR-EXP') * 0.9 $  

     10 OUTPUT,
        C'   DC SIGNAL Test-VOLTAGE
          --------------------------------------
          Connect DC1 output to the ', 'IMPEDANCE':5:2,
          C' Ohm load.

          T - Ready to test.
          F - Skip test. '
          $

     15 INPUT, INTO 'READY' $

        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 010000 $
        END, IF $
C $

C $
	OUTPUT, 'VOLTAGE':5:2, C' V is applied.\LF\'  $

     20 APPLY, DC SIGNAL,
           VOLTAGE 'VOLTAGE' RANGE -40 V TO 40 V,
           CURRENT-LMT 'CURR-LMT' RANGE 0 A TO 5 A,
           CNX HI DCPS1A-HI LO DCPS1A-LO $

	OUTPUT, 'CONTINUE' $
	INPUT, INTO 'READY' $

	CALCULATE, 'TESTNO' = 010120 $
        VERIFY, (VOLTAGE INTO 'VOLTAGE'), DC SIGNAL, UL 'VOLT-UL' V LL 'VOLT-LL' V,
	   VOLTAGE MAX 'VOLT-MAX' RANGE -40 V TO 40 V,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX SENSE-HI DCPS1A-SENHI SENSE-LO DCPS1A-SENLO   $
	CALCULATE, 'MEASUREMENT' = 'VOLTAGE' $
	PERFORM, 'V-RESULTS' $
        VERIFY, (CURRENT INTO 'CURRENT'), DC SIGNAL, UL 'CURR-UL' A LL 'CURR-LL' A,
	   CURRENT MAX 'CURR-MAX' RANGE 1 A TO 5 A,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX HI DCPS1A-HI LO DCPS1A-LO   $
	CALCULATE, 'MEASUREMENT' = 'CURRENT' $
	PERFORM, 'I-RESULTS' $
 
C $
	OUTPUT, C' \LF\',
          C'----------------------------------------          \LF\',
          C'The readback values are:              \LF\', 
	  C'Voltage  = ','VOLTAGE':5:2,C' V (+/-10%). \LF\',
	  C'Current  = ','CURRENT':5:2,C' A (+/-10%). \LF\',
	  C'                                                  \LF\',	
          C'T - Test Passed                                   \LF\',
          C'F - Test Failed                                   \LF\',
	  $
 
        
C $
        REMOVE,  DC SIGNAL,
           CNX HI DCPS1A-HI LO DCPS1A-LO $

	IF, 'E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI'$
        END, IF $

 
 010200	OUTPUT, 
	C'     DC SIGNAL TEST-CURRENT
              ------------------------\LF\' $
	OUTPUT, TO 'PRINTER',
	C'     DC SIGNAL TEST-CURRENT
              ------------------------\LF\' $

        
        
     10 OUTPUT,
        C'   DC SIGNAL Test- CURRENT
          --------------------------------------
          Connect DC1 output to the ', 'IMPEDANCE':5:2, C' Ohm load.

          T - Ready to test.
          F - Skip test. '
          $

     15 INPUT, INTO 'READY' $

        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 010000 $
        END, IF $
C $

c	CALCULATE, 'IMPEDANCE'    = 50 $
	CALCULATE, 'CURRENT'    = 2 $
        CALCULATE, 'VOLT-MAX' = 40 $
	CALCULATE, 'VOLT-LMT' = 40 $
	CALCULATE, 'VOLT-EXP' = 'CURRENT'*'IMPEDANCE' $
	CALCULATE, 'CURR-MAX'=5 $

	CALCULATE, 'VOLT-UL' = ('VOLT-EXP') * 1.1 $
        CALCULATE, 'VOLT-LL' = ('VOLT-EXP') * 0.9 $
	CALCULATE, 'CURR-UL' = ('CURRENT') * 1.1 $
        CALCULATE, 'CURR-LL' = ('CURRENT') * 0.9 $
C $
	OUTPUT, 'CURRENT':5:2, C' A is applied.\LF\'  $



     20 APPLY, DC SIGNAL,
           CURRENT 'CURRENT' RANGE 1 A TO 5 A,
           VOLT-LMT 'VOLT-LMT' RANGE -40 V TO 40 V,
           CNX HI DCPS1B-HI LO DCPS1B-LO 
                  SENSE-HI DCPS1B-SENHI SENSE-LO DCPS1B-SENLO $

	OUTPUT, 'CONTINUE' $
	INPUT, INTO 'READY' $
	CALCULATE, 'TESTNO' = 010220 $
        VERIFY, (VOLTAGE INTO 'VOLTAGE'), DC SIGNAL, UL 'VOLT-UL' V LL 'VOLT-LL' V,
	   VOLTAGE MAX 'VOLT-MAX' RANGE -40 V TO 40 V,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX SENSE-HI DCPS1B-SENHI SENSE-LO DCPS1B-SENLO $
	CALCULATE, 'MEASUREMENT' = 'VOLTAGE' $
	PERFORM, 'V-RESULTS' $
        VERIFY, (CURRENT INTO 'CURRENT'), DC SIGNAL, UL 'CURR-UL' A LL 'CURR-LL' A,
	   CURRENT MAX 'CURR-MAX' RANGE 1 A TO 5 A,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX HI DCPS1B-HI LO DCPS1B-LO $
	CALCULATE, 'MEASUREMENT' = 'CURRENT' $
	PERFORM, 'I-RESULTS' $
 
C $
	OUTPUT, C' \LF\',
          C'----------------------------------------          \LF\',
          C'The readback values are:              \LF\', 
	  C'Voltage  = ','VOLTAGE':5:2,C' V (+/-10%). \LF\',
	  C'Current  = ','CURRENT':5:2,C' A (+/-10%). \LF\',
	  C'                                                  \LF\',	
          C'T - Test Passed                                   \LF\',
          C'F - Test Failed                                   \LF\',
	  $
 
        
C $
	
        

        REMOVE,  DC SIGNAL,
           CNX HI DCPS1B-HI LO DCPS1B-LO 
                  SENSE-HI DCPS1B-SENHI SENSE-LO DCPS1B-SENLO $

	IF, 'E-E' EQ FALSE, THEN $
            OUTPUT, 'CONTINUE' $
            INPUT, INTO 'MI'$
        END, IF $


 010300	OUTPUT, 
	C'     DC SIGNAL TEST-NEGATIVE VOLTAGE
              ---------------------------------\LF\' $
	OUTPUT, TO 'PRINTER',
	C'     DC SIGNAL TEST-NEGATIVE VOLTAGE
              ---------------------------------\LF\' $

        
     10 OUTPUT,
        C'   DC SIGNAL Test-NEGATIVE VOLTAGE
          --------------------------------------
          Connect DC output to the ', 'IMPEDANCE':5:2, C' Ohm load.

          T - Ready to test.
          F - Skip test. '
          $

     15 INPUT, INTO 'READY' $

        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 010000 $
        END, IF $
C $
c	CALCULATE, 'IMPEDANCE'    = 50 $
	CALCULATE, 'VOLTAGE'    = -15 $
        CALCULATE, 'CURR-EXP' = -('VOLTAGE'/'IMPEDANCE') $
	CALCULATE, 'CURR-MAX'=5 $
        CALCULATE, 'CURR-LMT' = 4 $
	CALCULATE, 'MAX-T' = 10  $
	CALCULATE, 'VOLT-UL' = ('VOLTAGE') * .9 $
        CALCULATE, 'VOLT-LL' = ('VOLTAGE') * 1.1 $
	CALCULATE, 'CURR-UL' = ('CURR-EXP') * 1.1 $
        CALCULATE, 'CURR-LL' = ('CURR-EXP') * 0.9 $
C $
	OUTPUT, 'VOLTAGE':5:2, C' V is applied.\LF\'  $

     20 APPLY, DC SIGNAL,
           VOLTAGE 'VOLTAGE' RANGE -40 V TO 40 V,
           CURRENT-LMT 'CURR-LMT' RANGE 0 A TO 5 A,
           CNX HI DCPS1A-HI LO DCPS1A-LO $

	OUTPUT, 'CONTINUE' $
	INPUT, INTO 'READY' $

	CALCULATE, 'TESTNO' = 010320 $
        VERIFY, (VOLTAGE INTO 'VOLTAGE'), DC SIGNAL, UL 'VOLT-UL' V LL 'VOLT-LL' V,
	   VOLTAGE MAX 'VOLT-MAX' RANGE -40 V TO 40 V,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX SENSE-HI DCPS1A-SENHI SENSE-LO DCPS1A-SENLO   $
	CALCULATE, 'MEASUREMENT' = 'VOLTAGE' $
	PERFORM, 'V-RESULTS' $
        VERIFY, (CURRENT INTO 'CURRENT'), DC SIGNAL, UL 'CURR-UL' A LL 'CURR-LL' A,
	   CURRENT MAX 'CURR-MAX' RANGE 1 A TO 5 A,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX HI DCPS1A-HI LO DCPS1A-LO   $
	CALCULATE, 'MEASUREMENT' = 'CURRENT' $
	PERFORM, 'I-RESULTS' $
 
C $
	OUTPUT, C' \LF\',
          C'----------------------------------------          \LF\',
          C'The readback values are:              \LF\', 
	  C'Voltage  = ','VOLTAGE':5:2,C' V (+/-10%). \LF\',
	  C'Current  = ','CURRENT':5:2,C' A (+/-10%). \LF\',
	  C'                                                  \LF\',	
          C'T - Test Passed                                   \LF\',
          C'F - Test Failed                                   \LF\',
	  $
 
        
C $
        REMOVE,  DC SIGNAL,
           CNX HI DCPS1A-HI LO DCPS1A-LO $


     99 IF, 'E-E' EQ TRUE, THEN $
            GO TO, STEP 020100 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT, INTO 'MAIN-MENU' $

C GO BACK TO MAIN MENU OR RETEST $
        IF, 'MAIN-MENU' EQ TRUE, THEN $
	    GO TO, STEP 010000 $
	ELSE $ 
            GO TO, STEP 010100 $
        END, IF $
C $
C       END OF SHUTDOWN/LIMITING TESTING - LOOP BACK TO MENU $
        GO TO, STEP 001005 $
C $


C ************ DCPS_2 ************************** $
B $
 020100 OUTPUT, C' TESTS FOR SUPPLIES 2-10\LF\',
                C' THESE TESTS WILL BE TO SHOW COMMUNICATION WITH 
                   THE INSTRUMENT.  EXCEPTION: THE TESTS FOR 
                   SUPPLY 10 WILL ME MORE THOROUGH SINCE IT HAS 
                   SLIGHTLY DIFFERENT SPECS THAT 1 THRU 9 \LF\'  $
	OUTPUT, 
	C'     DCPS_2 TEST
              --------------\LF\' $
	OUTPUT, TO 'PRINTER',
	C'     DCPS_2 TEST
              --------------\LF\' $

	CALCULATE, 'IMPEDANCE'    = 13.5 $
	CALCULATE, 'VOLTAGE'    = 5 $
        CALCULATE, 'CURR-EXP' = 'VOLTAGE'/'IMPEDANCE' $
        CALCULATE, 'CURR-LMT' = 5 $
	CALCULATE, 'MAX-T' = 3.5  $
	CALCULATE, 'VOLT-UL' = ('VOLTAGE') * 1.1 $
        CALCULATE, 'VOLT-LL' = ('VOLTAGE') * 0.9 $
	CALCULATE, 'CURR-UL' = ('CURR-EXP') * 1.1 $
        CALCULATE, 'CURR-LL' = ('CURR-EXP') * 0.9 $

        OUTPUT,
        C'   DCPS_2 TEST
          --------------------------------------
          Connect DCPS_2 output to the ', 'IMPEDANCE':5:2, C' Ohm load.

          T - Ready to test.
          F - Skip test. '
          $

     18 INPUT, INTO 'READY' $

        IF, 'READY' EQ FALSE, THEN $
            GO TO, STEP 010000 $
        END, IF $
C $
	
C $
	OUTPUT, 'VOLTAGE':5:2, C' V is applied.\LF\'  $


     20 APPLY, DC SIGNAL,
           VOLTAGE 'VOLTAGE' RANGE -40 V TO 40 V,
           CURRENT-LMT 'CURR-LMT' RANGE 0 A TO 5 A,
           CNX HI DCPS2A-HI LO DCPS2A-LO 
                  SENSE-HI DCPS2A-SENHI SENSE-LO DCPS2A-SENLO $

	OUTPUT, 'CONTINUE' $
	INPUT, INTO 'READY' $

	CALCULATE, 'TESTNO' = 020120 $
        VERIFY, (VOLTAGE INTO 'VOLTAGE'), DC SIGNAL, UL 'VOLT-UL' V LL 'VOLT-LL' V,
	   VOLTAGE MAX 'VOLT-MAX' RANGE -40 V TO 40 V,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX SENSE-HI DCPS2A-SENHI SENSE-LO DCPS2A-SENLO $
	CALCULATE, 'MEASUREMENT' = 'VOLTAGE' $
	PERFORM, 'V-RESULTS' $
        VERIFY, (CURRENT INTO 'CURRENT'), DC SIGNAL, UL 'CURR-UL' A LL 'CURR-LL' A,
	   CURRENT MAX 'CURR-MAX' RANGE 1 A TO 5 A,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX HI DCPS2A-HI LO DCPS2A-LO $
	CALCULATE, 'MEASUREMENT' = 'CURRENT' $
	PERFORM, 'I-RESULTS' $

 
C $
	OUTPUT, C' \LF\',
          C'----------------------------------------          \LF\',
          C'The readback values are:              \LF\', 
	  C'Voltage = ','VOLTAGE':5:2,C' V (+/-10%). \LF\',
	  C'Current = ','CURRENT':5:2,C' A (+/-10%). \LF\',
	  C'                                                  \LF\',	
          C'T - Test Passed                                   \LF\',
          C'F - Test Failed                                   \LF\',
	  $
 
        
C $
	

        REMOVE,  DC SIGNAL,
           CNX HI DCPS2A-HI LO DCPS2A-LO 
                  SENSE-HI DCPS2A-SENHI SENSE-LO DCPS2A-SENLO $


     99 IF, 'E-E' EQ TRUE, THEN $
            GO TO, STEP 020200 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT, INTO 'MAIN-MENU' $

C GO BACK TO MAIN MENU OR RETEST $
        IF, 'MAIN-MENU' EQ TRUE, THEN $
	    GO TO, STEP 010000 $
	ELSE $ 
            GO TO, STEP 020100 $
        END, IF $
C $
C       END OF SHUTDOWN/LIMITING TESTING - LOOP BACK TO MENU $
        GO TO, STEP 001005 $


