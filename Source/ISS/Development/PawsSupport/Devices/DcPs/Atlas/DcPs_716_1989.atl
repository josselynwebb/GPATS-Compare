     20 APPLY, DC SIGNAL,
           VOLTAGE 'VOLTAGE' RANGE -40 V TO 40 V,
           CURRENT-LMT 'CURR-LMT' RANGE 0 A TO 5 A,
           CNX HI DCPS9A-HI LO DCPS9A-LO $
	OUTPUT, 'CONTINUE' $
	INPUT, INTO 'READY' $
	CALCULATE, 'TESTNO' = 020820 $
        VERIFY, (VOLTAGE INTO 'VOLTAGE'), DC SIGNAL, UL 'VOLT-UL' V LL 'VOLT-LL' V,
	   VOLTAGE MAX 'VOLT-MAX' RANGE -40 V TO 40 V,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX SENSE-HI DCPS9A-SENHI SENSE-LO DCPS9A-SENLO $
	CALCULATE, 'MEASUREMENT' = 'VOLTAGE' $
	PERFORM, 'V-RESULTS' $
        VERIFY, (CURRENT INTO 'CURRENT'), DC SIGNAL, UL 'CURR-UL' A LL 'CURR-LL' A,
	   CURRENT MAX 'CURR-MAX' RANGE 1 A TO 5 A,
           MAX-TIME 'MAX-T' RANGE 0 SEC TO 1000 SEC,
           CNX HI DCPS9A-HI LO DCPS9A-LO $
	CALCULATE, 'MEASUREMENT' = 'CURRENT' $
	PERFORM, 'I-RESULTS' $
 
C $
	OUTPUT, C' \LF\',
          C'----------------------------------------          \LF\',
          C'The readback values are:              \LF\', 
	  C'Voltage = ','VOLTAGE':5:2,C' V (+/-10%). \LF\',
	  C'Current = ','CURRENT':5:2,C' A (+/-10%). \LF\',
	  C'                                                  \LF\',	
          C'T - Test Passed                                   \LF\',
          C'F - Test Failed                                   \LF\',
	  $
         
C $

        REMOVE,  DC SIGNAL,
           CNX HI DCPS9A-HI LO DCPS9A-LO $


     99 IF, 'E-E' EQ TRUE, THEN $
            GO TO, STEP 020900 $
        END, IF $
C $
        OUTPUT, 'RETURN-MENU' $
        INPUT, INTO 'MAIN-MENU' $

C       GO BACK TO MAIN MENU OR RETEST $
        IF, 'MAIN-MENU' EQ TRUE, THEN $
	    GO TO, STEP 010000 $
	ELSE $ 
            GO TO, STEP 020800 $
        END, IF $
C $
C       END OF SHUTDOWN/LIMITING TESTING - LOOP BACK TO MENU $
        GO TO, STEP 001005 $


