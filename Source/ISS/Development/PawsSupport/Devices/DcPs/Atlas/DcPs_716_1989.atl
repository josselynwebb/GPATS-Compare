	  C'Voltage  = ','VOLTAGE':5:2,C' V (+/-10%). \LF\',
	  C'Current  = ','MEASUREMENT':5:2,C' A (+/-10%). \LF\',
 	  C'                                                  \LF\',	
          C'T - Test Passed                                   \LF\',
          C'F - Test Failed                                   \LF\',
	  $
 
        INPUT, INTO 'READY' $
C $

        REMOVE,  DC SIGNAL,
