  ************			    Sources                         ************
  ******************************************************************************
  begin using VEO_2_SOURCE;

    cnx via ATMOSPHERE;

    ****************************************************************************
    **********                  Infrared Sources                      **********
    ****************************************************************************
    begin FNC = 40;

      source infrared;

      control 
      {
        differential-temp range -10.0 degc to +10.0 degc by 0.001 degc;
        diff-temp-error range 0.001 degc to 5.0 degc;
        max-time range 0.0 sec to 380.0 sec;
        ** added new targets targ-4bar66, targ-4bar10, targ-cross17, targ-square21
        target-type targ-4bar33,
                    targ-4bar66,
                    targ-4bar10,
                    targ-4bar15,
                    targ-4bar267,
                    targ-4bar383,
                    targ-4bar5,
                    targ-brsght,
                    targ-cross17,
                    targ-diagln,
                    targ-etched,
                    targ-opnapr, 
                    targ-piesector,
                    targ-square21,
                    targ-tgtgrp04;
      }
    end;

    begin FNC = 41;

      source multi-sensor-infrared;

      control
      {
        settle-time 0.0 sec to 600.0 sec;
        target-type targ-irbs00,
                    targ-irbs01,
                    targ-irbs02,
                    targ-irbs03,
                    targ-irbs04,
                    targ-irbs05,
                    targ-irbs06,
                    targ-irbs07,
                    targ-irbs08,
                    targ-irbs09,
                    targ-irbs10;
      }

    end;

    ****************************************************************************
    **********                  Laser Sources                         **********
    ****************************************************************************
    begin FNC = 42;

      source laser target return;

      control
      {
        period range 0.050 sec to 0.125 sec;
        *pulse-width range 10.0e-9 sec to 70.0e-9 sec;
        pulse-width range 3.0e-9 sec to 30.0e-9 sec;
        target-range range 100.0 m to 10000.0 m;
        last-pulse-range range 100.0 m to 10000.0 m;
        trigger-mode lasr, auto;
        wave-length 1.064 um, 1.540 um, 1.570 um;
        power-dens range 0.5e-9 w/cm2 to 5.0e-6 w/cm2;
      }
    end;

    begin FNC = 43;

      source laser;

      control 
      {
        *sample-time 17.9e-6 sec to 33.3e-3 sec;
        *delay 0.0 sec to 6.553 sec;
        max-time range 0.0 sec to 300.0 sec;
        power-p range 0.0 nw to 5.0e6 nw; ** 0 nw to 5.0e6 nw
        *pulse-atten 0 to 4095;
        trigger-mode int, ext, lasr;
        *pulse-width range 3.0e-9 sec to 30.0e-9 sec;
        *pulse-width range 150.0e-9 sec, 2.4e-6 sec, 38.4e-6 sec, 614.4e-6 sec;
        wave-length 1.064 um, 1.540 um, 1.570 um;
      }
      begin;
        control period range 5.0e-3 sec to 200.0e-3 sec;
      end;
      begin;
        control prf range 5.0 hz to 200 hz;
      end;

    end;

    ****************************************************************************
    **********                 Visible Sources                        **********
    ****************************************************************************
    begin FNC = 44;
      source light;

      control
      {
        radiance range 0.0005 uw/cm2/sr to 5000.0 uw/cm2/sr by 0.00001 uw/cm2/sr;
        max-time range 0.0 sec to 180.0 sec;
        target-type targ-4bar66,
                    targ-4bar10,
                    targ-4bar33,
                    targ-4bar15,
                    targ-4bar267,
                    targ-4bar383,
                    targ-4bar5,
                    targ-brsght,
                    targ-cross17,
                    targ-diagln,
                    targ-etched,
                    targ-opnapr,
                    targ-piesector,
                    targ-square21,
                    targ-tgtgrp04;
      }

    end;

    begin FNC = 45;

      source multi-sensor-light;

      control
      {
        target-type targ-tvbs01,
                    targ-tvbs02,
                    targ-tvbs03,
                    targ-tvbs04,
                    targ-tvbs05,
                    targ-tvbs06,
                    targ-tvbs07,
                    targ-tvbs08,
                    targ-tvbs09,
                    targ-tvbs10,
                    targ-tvbs11,
                    targ-tvbs12,
                    targ-tvbs13,
                    targ-tvbs14,
                    targ-tvbs15;
        max-time range 0.0 sec to 10.0 sec;
      }
    end;
  end;
