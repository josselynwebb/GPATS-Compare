begin dev RF_CNTR_1;

  begin using COUNTER;

    cnx hi RFCntr-In;
    cnx via RFCntr-In;
    cnx event-strobe RFCntr-event-in;

    control
    {
      max-time range 0.1 sec to 1500 sec;
      event-strobe;
      **event-slope pos, neg;
      **event-delay range 0 sec to 100 sec;	** pending verification
    }
 
    begin;  *ac signal
      control
      {
        freq range 100.0e6 hz to 18.6e9 hz continuous;
        power range -37 dbm to 25 dbm;
        power range 0.1e-9 w to .316 w;
      }

      begin FNC = 1;  *freq
        sensor (freq) ac signal;
        control
        {
          peak-search 1;
          freq-window range 100.0e6 hz to 18.6e9 hz continuous; 
        }
      end;

      begin FNC = 2;  *period
        sensor (period) ac signal;
        control period range .000000250 sec to 1 sec continuous;
      end;

      begin FNC = 3;  *pulse-width
        sensor (pulse-width) ac signal;
        control pulse-width .000000050 sec to .5 sec continuous;
      end;

    end; *ac signal
  end; ** end COUNTER

  begin FNC = 100 using EVMON;

    event monitor (voltage-inst) dc signal;

    cnx hi RFTRIG-IN,
        event-out RfCntr_EvOut;

    control
    {
      event-out;
      event-slope pos, neg;
      max-time range 0 sec to 1000 sec;
      voltage-inst range 0 v to 10 v;
    }

  end; ** end event monitor

  begin FNC = 101 using EVDLY;

    event monitor (event-delay-value) event;

    cnx event-delay RfCntr_EvDlyIn,
        event-out   RfCntr_EvDlyOut;

    control
    {
      event-out;
      event-delay;
      event-delay-value range 0 sec to 100.0 sec; ** tentative value
      max-time range 0 sec to 1000 sec;
    }
  end;

end; *end RF_CNTR;
