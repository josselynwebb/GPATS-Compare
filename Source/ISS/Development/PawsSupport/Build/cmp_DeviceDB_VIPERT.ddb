    end; ** end EO Tests Infrared

    ****************************************************************************
    ************               Begin LASER Tests                    ************
    ****************************************************************************
    begin;
      **************************************************************************
      **********        Laser Measurements using Scope                **********
      **************************************************************************
      begin;
        control
        {
          sample-count range 1 to 128;
          main-beam-atten 0.0 od to 10.0 od;
          power-p range 1.0e-10 w to 633e-6 w;
          trig-level range -6.0 v to 6.0 v;
          trig-source ch1, ch2, ext, int;
          trig-slope pos, neg;
          sample-time 0.5e-6 sec, 1.0e-6 sec, 5.0e-6 sec,
                      10.0e-6 sec, 50.0e-6 sec, 100.0e-6 sec,
                      500.0e-6 sec, 1000e-6 sec;
          wave-length 1.540 um, 1.570 um, 1.064 um;
        }

        begin;
          sensor (pulse-energy) laser;
          control pulse-energy range 0.003 j to 0.3 j;
          begin FNC = 12;
            cnx via DSO-In1;
          end;
          begin FNC = 13;
            cnx via DSO-In2;
          end;
        end;

        begin;
          sensor (pulse-energy-stab) laser;
          control pulse-energy-stab range 0.0 pc to 100.0 pc;
          begin FNC = 14;
            cnx via DSO-In1;
          end;
          begin FNC = 15;
            cnx via DSO-In2;
          end;
        end;

        begin;
          sensor (power-p) laser;
          begin FNC = 16;
            cnx via DSO-In1;
          end;
          begin FNC = 17;
            cnx via DSO-In2;
          end;
        end;

        begin;
          sensor (pulse-ampl-stab) laser;
