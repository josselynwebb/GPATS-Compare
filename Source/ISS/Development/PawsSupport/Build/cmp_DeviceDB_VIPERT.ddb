********************************************************************************
begin dev RFGEN_1;

  cnx via RFStim-Out,
    event-strobe event-port;

  control
  {
    strobe-to-event;
    event-slope pos, neg;
    max-time range 0 sec to 19 sec;    
  }

  begin FNC = 100 using RFGEN_TTLTrg0;

    event monitor (voltage) dc signal;

    cnx EVENT-STROBE TTLTrig0,
        event-out event-port;*RFStim-Event;
        

    control
    {
       voltage range 0 v to 5 v;
       event-out;
       event-slope pos, neg;
    }

  end;

  begin FNC = 101 using RFGEN_TTLTrg1;

    event monitor (voltage) dc signal;

    cnx EVENT-STROBE TTLTrig1,
        event-out event-port;*RFStim-Event;

    control
    {
       voltage range 0 v to 5 v;
       event-out;
       event-slope pos, neg;
    }

  end;

  begin FNC = 102 using RFGEN_TTLTrg2;

    event monitor (voltage) dc signal;

    cnx EVENT-STROBE TTLTrig2,
        event-out event-port;*RFStim-Event;

    control
    {
       voltage range 0 v to 5 v;
       event-out;
       event-slope pos, neg;
    }

  end;

