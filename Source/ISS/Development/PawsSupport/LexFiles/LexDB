	constant;       NOUNRULE == 014;

define PSEUDO lexrule = NOUNRULE tables 1 = num 2 = type 3 = sub;

	COMMON MODIFIERS;

end_unit;


define NOUNS lexrule=NOUNRULE   string=3 tables 1=num 2=type 3=sub;

	AC SIGNAL                       "ACS";
