	DB/DEGF                                                 "DB/DEGF";
	DB/DEGK                                                 "DB/DEGK";
	DB/SEC                                                  "DB/SEC";

	DBM                                                     "DBM";
        DBW                                                     "DBW";
        DBK                                                     "DBK";

        DEG = *57.295787 RAD 
	    = *360.00    REV                                    "DEG";
