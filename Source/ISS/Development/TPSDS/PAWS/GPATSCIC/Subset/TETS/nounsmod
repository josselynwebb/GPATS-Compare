	begin NOUNS EXCHANGE;
	MODIFIERS:
			BUS-MODE;                       \\      BUSM
\\ NOW Dim              BIT;
			BUS-SPEC;                       \\      BUSS
    (LST_MC ++ XDT_MC)	COMMAND;                        \\      COMD
			COMMAND-WORD;                   \\      CMDW**
			COMMAND-WORD-BIT;               
    (LST_MC ++ XDT_MC)  DATA;                           \\      DATA
			DATA-WORD;                      \\      DATW**
			DATA-WORD-BIT;
			DATA-WORD-LENGTH;
			DATA-WORD-PARITY;
			DATA-WORD-SYNC;
			DATA-WORD-TR;
                        DELAY;                          \\      DELY
			EXCHANGE-NUMBER;
                        FAULT-COUNT;                    \\      FLTC
			FAULT-TEST;                     \\      FLTS**
			INVALID-COMMAND-WORD;
			INVALID-COMMAND-WORD-BIT;
			INVALID-DATA-WORD;
			INVALID-DATA-WORD-BIT;
			INVALID-DATA-WORD-LENGTH;
			INVALID-DATA-WORD-PARITY;
			INVALID-DATA-WORD-SYNC;
			INVALID-DATA-WORD-TR;
			INVALID-MESSAGE-GAP;
			INVALID-ONE-AMPLITUDE;
                        INVALID-RESPONSE-TIME;
			INVALID-STATUS-WORD;            \\ 
			INVALID-STATUS-WORD-BIT;
			INVALID-WORD-COUNT;             
			INVALID-WORD-GAP;               
			INVALID-WORD-LENGTH;            
			INVALID-ZERO-AMPLITUDE;         
			INVALID-ZERO-CROSSING;          
\\\\ NOW Dim            LENGTH;                         \\      LNTH**
			MESSAGE-GAP;                    \\      MGAP
			ONE-AMPLITUDE;                  \\      OAMP
\\\\ Now Dim            PARITY;                         \\      PRTY**
			PROCEED;                        \\      PRCD**
			RESPONSE-TIME;                  \\      RSPT
