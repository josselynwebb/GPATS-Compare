print "Source File: nounsmods";

\\
\\       The following defines the <noun> / <modifier> relationships.
\\

	begin PSEUDO COMMON MODIFIERS;
	MODIFIERS    :
			CURRENT-LMT;
			DELAY;
			MAX-TIME;
			RESP;
			NEG-SLOPE;
			POS-SLOPE;
			VALUE;  
			VOLT-LMT;
			WORD-TRANSITION;
			BIT-TRANSITION;
    (DSC_MC)		CLOCK-SOURCE;		\\MCATES
			TEST-EQUIP-IMP;		\\MCATES
			TRIG-LEVEL;
    (DSC_MC)		TRIG-SLOPE;		\\MCATES
    (DSC_MC)		TRIG-SOURCE;		\\MCATES
			INTERVAL;
    (DSC_MC)		COUPLING;
            		ATTENUATION;
			FILTER;
    (IDT_MC)		POSITION;		\\MCATES
    (DSC_MC)		DISPLAY;		\\MCATES
    (DSC_MC)		SMOOTH;			\\MCATES
\\
\\      EventMonitor
\\
			\\
			\\      Compiler Generates SET <EventIdentifier>
			\\
			EVENT-OUT;
			EVENT-AFTER-OCCURRENCES;
			EVENT-DELAY;
			EVENT-EACH-OCCURRENCE;
			EVENT-FIRST-OCCURRENCE;
			EVENT-GATE-FROM;
			EVENT-GATE-TO;
			EVENT-GATED-BY;
			EVENT-SENSE;
			EVENT-STIM;
\\
\\      EVENT-STROBE is an Alias for STROBE-TO-EVENT
\\
\\                      EVENT-STROBE;
                        STROBE-TO-EVENT;
\\
\\      EVENT-SYNC is an Alias for SYNC
\\
\\                      EVENT-SYNC;
			SYNC;
			EVENT-TIME-FROM;
			EVENT-TIME-TO;
			EVENT-WHEN;

			EVENT-SYNC-WORD;
			EVENT-SYNC-BIT;
			EVENT-SYNC-FREQ;

			\\
			\\      Exchange Frame ONLY
			\\
\\\\                    EVENT-EVERY;
\\\\                    EVENT-UNTIL;

                        \\
			\\      Compiler Generates SET <Value>
			\\
		       EVENT-AFTER-OCCURRENCES-VALUE;
		       EVENT-DELAY-VALUE;
		       EVENT-EACH-OCCURRENCE-VALUE;
		       EVENT-FIRST-OCCURRENCE-VALUE;
		       EVENT-SLOPE;
		       \\
		       \\       Compiler Generates SET <Value>
		       \\
		       EVENT-GATE-FOR;
		       EVENT-TIME-FOR;
		       \\
		       \\       Compiler Generates SET
		       \\
		       EVENT-GATE-FOREVER;
		       EVENT-TIME-FOREVER;
		       \\
		       \\       Signal <Mchar> Only
		       \\
		       EVENT-INDICATOR;
		       MANUAL-INTERVENTION;
\\
\\      DIGITAL CONFIGURATION
\\
			BIT-PERIOD;
			BIT-RATE;
	  (XDT_MC)      ERROR;
	  (IDT_MC)      ERROR-INDEX;
	  (IDT_MC)      EXCEPT AT EVERY;
	  (IDT_MC)      FAULT-COUNT;
                        ILLEGAL-STATE-INDICATOR;
	  (XDT_MC)      MASK-ONE;
	  (XDT_MC)      MASK-ZERO;
			MINIMUM-SENSE-RATE;
			PULSE-CLASS;
                        SKEW-TIME;
   (LST_MC ++ XDT_MC)   REF;
	  (IDT_MC)      REPEAT;
	  (XDT_MC)      SAVE-COMP;
                        SERIAL-LSB-FIRST;
			SERIAL-MSB-FIRST;
			VOLTAGE-ONE;            \\ AWN
			VOLTAGE-ZERO;           \\ AWN
			CURRENT-ONE;            \\ AWN
			CURRENT-ZERO;           \\ AWN
                        STIM;
	  (DSC_MC)      TYPE;
			WORD-LENGTH;
			VOLTAGE;
			PATH;
\\
\\ DO, SIMULTANEOUS
\\
			DO-SIMULTANEOUS;

\\
\\ Define Digital S/S: External Specification
\\
			DIG-SPEC;
\\
	end_unit;

