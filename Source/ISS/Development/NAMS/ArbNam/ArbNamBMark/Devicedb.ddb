begin dev RF_GEN;
  cnx via RFStim-Out;

  begin FNC = 0;
    source ac signal;
    control
      {
      power range -90 dbm to 10 dbm continuous;
      freq range 10 mhz to 8500 mhz continuous;
      freq-window range 10 mhz to 8500 mhz continuous;
      age-rate range 10000000 hz/sec to 8490000000 hz/sec continuous;
      }
  end;

  begin FNC = 1;
    source fm signal;
    control
      {
      car-ampl -90 dbm to 10 dbm continuous;
      car-freq range 10 mhz to 8500 mhz continuous;
      }
  end;

  begin FNC = 2;
    source am signal;
    control
      {
      car-ampl -90 dbm to 10 dbm continuous;
      car-freq range 10 mhz to 8500 mhz continuous;
      mod-ampl range 0 pc to 90 pc continuous;
      }

  end;

  begin FNC = 3;
    source pulsed ac;
    control
      {
      power range -90 dbm to 10 dbm continuous;
      car-freq range 10 mhz to 8500 mhz continuous;
      pulse-width range 0.00000005 sec to 1000 sec continuous;
      }
  end;
end; *end RF_GEN
**************************************************************************
************      BU65550 (DDC1553) STATIC DESCRIPTION        ************
**************************************************************************
begin dev DDC1553 using BU65550;

  cnx true DDC1553b-hi, compl DDC1553b-lo;

  begin FNC=1;
    function bus protocol;
    control
      {
      bus-spec mil-1553b;
      standard primary;
      max-time range 0 sec to 10 sec continuous;
      proceed;
      test-equip-role master, monitor, slave;
      wait;
      proceed;
      }
  end;

  begin FNC=2;  ** Bus Monitor Mode
    function exchange;
    control
      {
      bus-mode con-rt,
               rt-con,
               con-mode;
      test-equip-role monitor;
      uut-talker max 0;
      }
  end;

  begin FNC=3;  ** Bus Controller Mode 
    function exchange;
    control
      {
      bus-mode con-rt,
               rt-con,
               con-mode;
      test-equip-role master;
      test-equip-talker max 0;
      uut-talker max 1;
      test-equip-listener max 0;
      uut-listener range 0 to 3;
      data max 32;
      command max 2;
      delay range 0 usec to 65535 usec;
      }
  end;

  begin FNC=4;  ** Remote Terminal Mode
    function exchange;
    control
      {
      bus-mode con-rt,
               rt-con,
               con-mode;
      test-equip-role slave;
      test-equip-listener max 33;
      test-equip-talker max 33;
      uut-listener max 33;
      uut-talker max 33;
      data max 32;
      status max 2;

      }
  end;

end; *end DDC1553

