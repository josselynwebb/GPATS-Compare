            beam-detection-threshold range 0.001 db to 28.0 db;
	    satellite-beam-atten range 0 od to 10 od;
	    main-beam-atten range 0 od to 10 od;
     	  }
      end;

    end;	** End laser-category-1 Tests

    begin using laser-category-2;

      cnx via ATMOSPHERE;
      control
      {
         wave-length 1.064 um, 1.54 um, 1.57 um;                   *ARM_Diode_Selection
         target-range range 15.0 m to 40000.0 m;	           *ARM_First_Pulse_Range
         pulse-width range 20.0e-9 sec to 150.0e-9 sec continuous; *ARM_Pulse_Width
      }

      begin FNC = 44;
        sensor(receiver-sensitivity) laser;
    	  control
     	  {
	       receiver-sensitivity range 1.0E-10 w/cm2 to 5.0E-6 w/cm2;
	     range-error range 1 m to 10000 m;
	     test-point-count 1 to 17 by 1;
   	  }
      end;

      begin FNC = 45;
        sensor(range-error) laser;
    	  control
     	  {
	 	  range-error range 0 m to 40000 m;
             power-dens range 1.0E-10 w/cm2 to 5.0E-6 w/cm2;           *ARM_Diode_Amplitude
             last-pulse-range range 19.5 m to 40000.0 m;	       *ARM_Second_Pulse_Range
   	  }
      end;

    end;	** End laser-category-2 Tests

	*****************************************************************
	********  IR/VIS Boresight/ORL Measurements using ALTA   ********
	*****************************************************************
    begin using alta-ir-vis-boresight;

      cnx via VIDEO-IN;
      control 
      { 
         h-field-of-view range 1e-9 rad to 3.1416 rad continuous;
         v-field-of-view range 1e-9 rad to 3.1416 rad continuous;
         h-target-angle range 1e-9 rad to 3.1416 rad continuous;
         v-target-angle range 1e-9 rad to 3.1416 rad continuous;
         h-target-offset range -1.5708 rad to 1.5708 rad;
         v-target-offset range -1.5708 rad to 1.5708 rad;
         tgt-coordinate-left range 0 to 1254 by 1 ;
         tgt-coordinate-top range 0 to 942 by 1;       
         tgt-coordinate-right range 0 to 1254 by 1;
         tgt-coordinate-bottom range 0 to 942 by 1;
         intensity-ratio range 0.0 to 99.0 pc;
         sample-av range 1 frames to 128 frames;       
         format rs343-675-1-1, rs343-675-4-3, rs343-729-1-1, rs343-729-4-3,
                rs343-875-1-1, rs343-875-4-3, rs343-945-1-1, rs343-945-4-3,
                rs343-1023-1-1, rs343-1023-4-3, rs170;
      } 
                
      begin FNC = 50;
        sensor(diff-boresight-angle) infrared;
          control
          {
	  	      diff-boresight-angle range -1.5708 rad to 1.5708 rad;
            x-boresight-angle range -1.5708 rad to 1.5708 rad;
            y-boresight-angle range -1.5708 rad to 1.5708 rad;
            h-los-align-error range -1.5708 rad to 1.5708 rad;
            v-los-align-error range -1.5708 rad to 1.5708 rad;  
	    settle-time 0.0 sec to 600.0 sec;
            target-type tets-irbs00, tets-irbs01, tets-irbs02, tets-irbs03, tets-irbs04,
                        tets-irbs05, tets-irbs06, tets-irbs07, tets-irbs08, tets-irbs09,
                        tets-irbs10;
          }
      end;

      begin FNC = 51;
        sensor(diff-boresight-angle) light;
          control
          {
	  	  diff-boresight-angle range -1.5708 rad to 1.5708 rad;
            x-boresight-angle range -1.5708 rad to 1.5708 rad;
            y-boresight-angle range -1.5708 rad to 1.5708 rad;
            h-los-align-error range -1.5708 rad to 1.5708 rad;
            v-los-align-error range -1.5708 rad to 1.5708 rad;  
            target-type tets-tvbs01, tets-tvbs02, tets-tvbs03, tets-tvbs04, tets-tvbs05,
                        tets-tvbs06, tets-tvbs07, tets-tvbs08, tets-tvbs09, tets-tvbs10,
                        tets-tvbs11, tets-tvbs12, tets-tvbs13, tets-tvbs14, tets-tvbs15;
          }
      end;

      begin FNC = 52;
        sensor(los-align-error) infrared;
          control
          {
		    los-align-error range -1.5708 rad to 1.5708 rad;
            x-autocollimation-error range -1.0 rad to 1.0 rad;
            y-autocollimation-error range -1.0 rad to 1.0 rad;
	    settle-time 0.0 sec to 600.0 sec;
            target-type tets-irbs00, tets-irbs01, tets-irbs02, tets-irbs03, tets-irbs04,
                        tets-irbs05, tets-irbs06, tets-irbs07, tets-irbs08, tets-irbs09,
                        tets-irbs10;
          }
      end;

      begin FNC = 53;
        sensor(los-align-error) light;
      	  control
    	  {
	 	    los-align-error range -1.5708 rad to 1.5708 rad;
            x-autocollimation-error range -1.0 rad to 1.0 rad;
            y-autocollimation-error range -1.0 rad to 1.0 rad;
            target-type tets-tvbs01, tets-tvbs02, tets-tvbs03, tets-tvbs04, tets-tvbs05,
                        tets-tvbs06, tets-tvbs07, tets-tvbs08, tets-tvbs09, tets-tvbs10,
                        tets-tvbs11, tets-tvbs12, tets-tvbs13, tets-tvbs14, tets-tvbs15;
          }
      end;

    end;	** End IR/VIS Boresight/ORL Measurements

	*****************************************************************
	********      Autocollimation Measurement - LASER        ********
	*****************************************************************
    begin using laser-autocollimation;

      cnx via LASER-CAMERA;

      begin FNC = 54;
        sensor(autocollimation-error) laser;
          control
          {
	      autocollimation-error range -1.0 rad to 1.0 rad;
            intensity-ratio range 0.0 to 99.0 pc;
            sample-av range 1 frames to 128 frames;       	     *Image_Num_Frames
            delay range 0.0 sec to 6.553 sec by .000002 sec;	     *Camera_Delay_Time
            sample-time range 19.0e-6 sec to 0.0331 sec;	     *Camera_Integration_Time
            trigger-mode int, ext, lasr;			     *Camera_Trigger
            wave-length 1.064 um, 1.54 um, 1.57 um;
            pulse-width 150.0e-9 sec, 2.4e-6 sec, 38.4e-6 sec, 614.4e-6 sec;
            pulse-atten range 0 to 4095 by 1;
          }
      end;
    end;	** End Autocollimation Measurements

	*****************************************************************
	********      Pulse Energy Measurements - LASER          ********
	*****************************************************************
    begin using laser-dso-ch1;
 
      cnx via DSO-In1;
      control
      {
	*SC_Input_Channel = 1
        sample-count range 1 to 128 by 1;		    *SC_Number_Measurements
        trig-level range -6.0 v to 6.0 v continuous;	    *SC_Trigger_Level
        trig-source ch2, ext, int;			    *SC_Trigger_Source
        trig-slope pos, neg;				    *SC_Trigger_Slope
        sample-time 500e-9 sec, 1e-6 sec, 5e-6 sec, 10e-6 sec, 50e-6 sec, 100e-6 sec, 500e-6 sec, 1e-3 sec; *SC_Timebase
        main-beam-atten range 0 od to 10 od;
        wave-length 1.064 um, 1.54 um, 1.57 um;
        power-p range 0.02 w to 20.0e6 w continuous;           *SC_Input_Range
      }

      begin FNC = 80;
        sensor (pulse-ampl-stab) laser;		*Amplitude_Instability (3)
        control
          {
	    pulse-ampl-stab range 0.0 pc to 100.0 pc;
          }
      end;

      begin FNC = 81;
        sensor (pulse-energy) laser;		*Average_Energy (2)
        control
          {
	    pulse-energy range 0.003 j to 0.300 j;
          }
      end;

      begin FNC = 82;
        sensor (pulse-width) laser;		*Average_Pulse_Width (6)
        control
          {
	    pulse-width range 4.0 nsec to 70.0 nsec;
          }
      end;

      begin FNC = 83;
        sensor (power-p) laser;			*Average_Amplitude (3)
      end;

      begin FNC = 84;
        sensor (pulse-energy-stab) laser;	*Energy_Instability (1)
        control
          {
	    pulse-energy-stab range 0.0 pc to 100.0 pc;
          }
      end;

    end;	*laser-scope-ch1

    begin using laser-dso-ch2;
 
      cnx via DSO-In2;
      control
      {
	*SC_Input_Channel = 2
        sample-count range 1 to 128 by 1;		    *SC_Number_Measurements
        trig-level range -6.0 v to 6.0 v continuous;	    *SC_Trigger_Level
        trig-source ch1, ext, int;			    *SC_Trigger_Source
        trig-slope pos, neg;				    *SC_Trigger_Slope
        sample-time 500e-9 sec, 1e-6 sec, 5e-6 sec, 10e-6 sec, 50e-6 sec, 100e-6 sec, 500e-6 sec, 1e-3 sec; *SC_Timebase
        main-beam-atten range 0 od to 10 od;
        wave-length 1.064 um, 1.54 um, 1.57 um;
        power-p range 0.02 w to 20.0e6 w continuous;           *SC_Input_Range
      }

      begin FNC = 85;
        sensor (pulse-energy) laser;		*Average_Energy (2)
        control
          {
	    pulse-energy range 0.003 j to 0.300 j;
          }
      end;

      begin FNC = 86;
        sensor (pulse-width) laser;		*Average_Pulse_Width (6)
        control
          {
	    pulse-width range 4.0 nsec to 70.0 nsec;
          }
      end;

      begin FNC = 87;
        sensor (power-p) laser;			*Average_Amplitude (4)
      end;

      begin FNC = 88;
        sensor (pulse-energy-stab) laser;	*Energy_Instability (1)
        control
          {
	    pulse-energy-stab range 0.0 pc to 100.0 pc;
          }
      end;

      begin FNC = 89;
        sensor (pulse-ampl-stab) laser;		*Amplitude_Instability (3)
        control
          {
	    pulse-ampl-stab range 0.0 pc to 100.0 pc;
          }
      end;

    end;	*laser-scope-ch2

	*****************************************************************
	********      Pulse Repitition Measurements - LASER      ********
	*****************************************************************

    begin using laser-cntr-ch1;

      cnx via CT-In1;
      control
       {
         sample-count range 1 to 128 by 1;		    *TC_Number_Measurements
         trig-level range 0.0 v to 1.0 v continuous;	    *TC_Trigger_Level
         delay range 0.0 sec to 1.0 sec by 0.001 sec;	    *TC_Delay
	 power-p range 0.02 w to 20.0e6 w continuous;      
         main-beam-atten range 0 od to 10 od;
         wave-length 1.064 um, 1.54 um, 1.57 um;
       }

      begin FNC = 90;
        sensor (pulse-period-stab) laser;		    *Period_Instability (1)
        control
          {
	    pulse-period-stab range 0.0 pc to 100.0 pc;
          }
      end;

      begin FNC = 91;
        sensor (prf) laser;				    *Average_Period (2)
        control
          {
	    prf range 8.0 pps to 25.0 pps;
          }
      end;

    end;	*laser-cntr-ch1

  end;	    ** end laser

