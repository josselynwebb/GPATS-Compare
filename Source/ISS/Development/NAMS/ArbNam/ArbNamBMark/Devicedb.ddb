begin dev DWG;

  ** Digital configuration
  begin FNC = 1;
    function digital configuration;
    cnx hi DWGX;
    cnx event-sense DWGCSE;
    cnx event-stim  DWGCST;
    control
      {
        stim-rate   range 8 kwords/sec to 25000        kwords/sec;
        sense-rate  range 8 kwords/sec to 25000        kwords/sec;   
        sense-delay range 0 sec        to     0.131072 msec;
        max-time    range 0 sec        to   100        sec;   ** TBD
        iterate     max   262144 times;
        do-timed-digital on,off;
        event-sense;
        event-stim;
      }
  end;

  ** Stimulate static Logic Data
  begin FNC = 10;
    function logic data;
    cnx hi DWGX;
    control
      {
        type          parallel;
        stim          range  1     to 32767;
        voltage-zero  range -2 v   to     5 v;
        voltage-one   range -2 v   to     5 v;
        voltage-quies range -2 v   to     5 v;
        fall-time     range  0 sec to     1 sec;
        rise-time     range  0 sec to     1 sec;
        stim-zero;
        stim-one;
        stim-hiz;
        illegal-state-indicator;
        level-logic-zero  voltage range  -2 v   to     5 v;
        level-logic-one   voltage range  -2 v   to     5 v;
        repeat range 1 to 32767;  
      }
  end;

  ** Sense static Logic Data
  begin FNC = 17;
    cnx hi DWGX;
    function-value logic data;
    control
      {
        type          parallel;
        value         range    0    to  32767;
        hiz           range    0    to  32767;
        voltage-zero  range -2 v    to      5 v;
        voltage-one   range -2 v    to      5 v;
        voltage-quies range -2 v    to      5 v;
        current-one   range -10 ma  to     10 ma;   ** TBD
        current-zero  range -10 ma  to     10 ma;   ** TBD
        word-count    max 32767;
        fault-count   1;
      }
  end;

  ** Prove static Logic Data
  begin FNC = 22;
    cnx hi DWGX;
    function-value logic data;
    control
      {
        type          parallel;
        value         range  0    to  32767;
        hiz           range  0    to  32767;
        ref           range  0    to  32767;
        mask-one      range  0    to  32767;
        mask-zero     range  0    to  32767;
        save-comp     range  0    to  32767;
        error         range  0    to  32767;
        error-index   range  0    to  32767;
        fault-count   1;
        word-count    max 32767;
        voltage-zero  range -2 v    to      5 v;
        voltage-one   range -2 v    to      5 v;
        voltage-quies range -2 v    to      5 v;
        current-one   range -10 ma  to     10 ma;   ** TBD
        current-zero  range -10 ma  to     10 ma;   ** TBD
      }
  end;

  ** Apply static Logic Data (regular channels)
  begin FNC = 11;

    source logic data;
    cnx hi DS-HI;
    control
    {
      voltage-zero range -2 v to 5 v;
      voltage-one  range -2 v to 5 v;
      value        range  0   to 192;
    }
  end;

  ** Measure static Logic Data (regular channels)
  begin FNC = 12;

    sensor (value) logic data;
    cnx hi DS-HI;
    control
    {
      voltage-zero range -2 v to 5 v;
      voltage-one  range -2 v to 5 v;
      value        range  1   to 32768;
    }
  end;

  ** Apply static Logic Data (regular channels)
  begin FNC = 31;

    source logic data;
    cnx hi SCAN-HI;
    control
    {
      voltage-zero 0 v;  ** Changed by Q.Nguyen on 10/10/00 per Ticket# 347
      voltage-one  5 v;  ** Changed by Q.Nguyen on 10/10/00 per Ticket# 347
      value        range  0   to 32;    ** !! TBD
    }
  end;

  ** Measure static Logic Data (regular channels)
  begin FNC = 32;

    sensor (value) logic data;
    cnx hi SCAN-HI;
    control
    {
      voltage-zero range 0 v to 10.235 v;   ** Changed by Q.Nguyen on 10/10/00 per Ticket# 347
      voltage-one  range 0 v to 10.235 v;   ** Changed by Q.Nguyen on 10/10/00 per Ticket# 347
      value        range  1   to 32768; ** !! TBD
    }
  end;

  *********** Events ********************
  
  ** Event based event (EBE) calling TBE or SBE (2 layers)
  begin FNC = 51;
    event monitor (event-delay-value) event;
    cnx event-out   DWGEBE51;
    cnx event-delay DWGT2;
    control
      {
        event-out;
        event-delay;
        event-delay-value  range 0.0 sec to 10.0 sec;  ** TBD 
      }
  end;

  ** Event based event (EBE) calling EVE (3 layers)
  begin FNC = 52;
    event monitor (event-delay-value) event;
    cnx event-out   DWGEBE52;
    cnx event-delay DWGE3;
    control
      {
        event-out;
        event-delay;
        event-delay-value  range 0.0 sec to 10.0 sec;  ** TBD 
      }
  end;

  ** Event based event (EBE) calling TBE or SBE (3 layers)
  begin FNC = 53;
    event monitor (event-delay-value) event;
    cnx event-out   DWGEBE53;
    cnx event-delay DWGT3;
    control
      {
        event-out;
        event-delay;
        event-delay-value  range 0.0 sec to 10.0 sec;  ** TBD 
      }
  end;

  ** Timer based event (TBE) (2 layer)
  begin FNC = 62;
    event monitor (time) timer;
    cnx event-out   DWGTBE62;
    control
      {
        event-out;
        time range 0 sec to 100 sec;  ** TBD
      }
  end;

  ** Signal based event (SBE) (2 layer)
  begin FNC = 72;
    event monitor (voltage) square wave;
    cnx event-out DWGSBE72;
    cnx hi;
    control
      {
        event-out;
        voltage    range -2 v to 5 v;
        period     range 0 sec to 100 sec;  ** TBD
      }
  end;

  ** Timer based event (TBE) (3 layer)
  begin FNC = 63;
    event monitor (time) timer;
    cnx event-out   DWGTBE63;
    control
      {
        event-out;
        time range 0 sec to 100 sec;  ** TBD
      }
  end;

  ** Signal based event (SBE) (3 layer)
  begin FNC = 73;
    event monitor (voltage) square wave;
    cnx event-out DWGSBE73;
    cnx hi;
    control
      {
        event-out;
        voltage    range -2 v to 5 v;
        period     range 0 sec to 100 sec;  ** TBD
      }
  end;

  ** Timer
  begin FNC = 80;
    timer (time) timer;
    control
      {
        time range 0 to 100 sec;  ** TBD
      }
  end;

end; ** dev DWG
**************************************************************************
************    HP E1445A (ARB_GEN) STATIC DESCRIPTION        ************
**************************************************************************
begin dev ARB_GEN;
  
  begin ; ** event-monitor
   event monitor (voltage) square wave;
     cnx event-out ARB-EvArm;
     
     control
      {
        event-out;
        delay range 0.1 sec to 1000 sec;
        event-slope pos, neg;	
      }

     begin FNC = 102; 		** External trigg.
      cnx hi ARB-StopTrigFsk;
     end;

  end;	** end event-monitor

  begin using HPE1445A;

    control
      {
        max-time range 0 sec to 1000 sec continuous;
      }
    cnx via ARB-Out,
	div7-out ARB-OUT/7;
    cnx event-sync ARB-EvArm;

    begin FNC=1; 		* sine wave
      source ac signal;

	control
      	{
            freq range .01 hz to 10737418.24 hz continuous;
            bandwidth 250000 hz, 10000000 hz;
            event-sync;
	    freq-window range 0.01 hz to 10737418.24 hz continuous;
	    age-rate range 1 hz/sec to 3350 hz/sec continuous;
      	}

	begin;
 	  control
          {
            voltage-pp range .32374 v to 10.2375 v continuous;
            dc-offset range -5 v to 5 v continuous;
            test-equip-imp 50 ohm, 75 ohm;
          }
	end;

	begin;
          control
          {
            voltage-pp range .64748 v to 20.475 v continuous;
            dc-offset range -10 v to 10 v continuous;
            test-equip-imp 1 mohm;
          }
	end;

    end;

    begin FNC=2; 		* square wave
      source square wave;

	control
      	{
            freq range .0025 hz to 5368709.12 hz continuous;
            bandwidth 250000 hz, 10000000 hz;
            event-slope pos, neg;	
            event-sync;
	}

	begin;
          control
          {
            voltage-pp range .32374 v to 10.2375 v continuous;
            dc-offset range -5 v to 5 v continuous;
            test-equip-imp 50 ohm, 75 ohm;
          }
	end;

	begin;
          control
          {
            voltage-pp range .64748 v to 20.475 v continuous;
            dc-offset range -10 v to 10 v continuous;
            test-equip-imp 1 mohm;
          }
	end;

    end;

    begin FNC=3; 		* ramp waveform
      source ramp signal ;

	control
      	{
            freq range .0002 hz to 5368709.12 hz continuous;
            bandwidth 250000 hz, 10000000 hz;
            event-slope pos, neg;	
            event-sync;
	}

	begin;
          control
          {
            voltage-pp range .32374 v to 10.2375 v continuous;
            dc-offset range -5 v to 5 v continuous;
            test-equip-imp 50 ohm, 75 ohm;
          }
	end;

	begin;
          control
          {
            voltage-pp range .64748 v to 20.475 v continuous;
            dc-offset range -10 v to 10 v continuous;
            test-equip-imp 1 mohm;
          }
	end;

    end;

    begin FNC=4; 		* user waveform
      source waveform;

      control
        {
           stim range 4 to 262144 by 1;
           sample-spacing 47 nsec to 100 sec continuous;
           bandwidth 250000 hz, 10000000 hz;
           event-sync;
        }

	begin;
          control
          {
            dc-offset range -5 v to 5 v continuous;
            test-equip-imp 50 ohm, 75 ohm;
          }
	end;

	begin;
          control
          {
            dc-offset range -10 v to 10 v continuous;
            test-equip-imp 1 mohm;
          }
	end;
    end;

    begin FNC=6; 		* triangular waveform
      source triangular wave signal;

	control
      	{
            freq range .0002 hz to 5368709.12 hz continuous;
            bandwidth 250000 hz, 10000000 hz;
            event-slope pos, neg;	
            event-sync;
	}

	begin;
          control
          {
            voltage-pp range .32374 v to 10.2375 v continuous;
            dc-offset range -5 v to 5 v continuous;
            test-equip-imp 50 ohm, 75 ohm;
          }
	end;

	begin;
          control
          {
            voltage-pp range .64748 v to 20.475 v continuous;
            dc-offset range -10 v to 10 v continuous;
            test-equip-imp 1 mohm;
          }
	end;

    end;

    begin FNC=7; 		* dc signal
      source dc signal;

	begin;
          control
          {
            voltage range -5.12 v to 5.11875 v continuous;
            test-equip-imp 50 ohm, 75 ohm;
          }
	end;

	begin;
          control
          {
            voltage range -10.24 v to 10.2375 v continuous;
            test-equip-imp 1 mohm;
          }
	end;

    end;

  end;
end; *end arb_gen
**************************************************************************
************  HP E1420B (COUNTER-TIMER) STATIC DESCRIPTION    ************
**************************************************************************
begin dev COUNTER_TIMER;

  begin using cntr-ch1;
    cnx via CT-In1;
    cnx hi CT-In1;
    cnx event-gate-from CT-EvArm;
    cnx event-gate-to CT-EvArm;
    cnx event-strobe event-port;
    control
    {
      coupling ac, dc;
      voltage range .1 v to 100 v continuous;
      test-equip-imp 50 ohm, 1000000 ohm;
      pos-slope;
      neg-slope;
      event-gate-from;
      event-gate-to;
      strobe-to-event;
    }

    begin;  *ac signal
      control
      {
        freq range .001 hz to 200000000 hz continuous;
        period range .000000005 sec to 1000 sec continuous;
        freq-ratio range .00000000001 to 100000000000 continuous;
	phase-angle range 0 deg to 359.9 deg;
      }
  
      begin FNC=1;  *freq
        sensor (freq) ac signal ;
      end;
    
      begin FNC=2;  *period
        sensor (period) ac signal;
      end;
 
      begin FNC=3;  *ratio
        cnx-ref via CT-In2;
        sensor (freq-ratio) ac signal;
      end;

      begin FNC=15;  *phase angle
        cnx-ref via CT-In2;
        sensor (phase-angle) ac signal;
      end;

    end; *ac signal

    begin;  *pulsed dc
      control
      {
        rise-time range .000000015 sec to .001 sec continuous; **Changed per ECP4 Change 4 
        fall-time range .000000015 sec to .001 sec continuous; **25 APR 00 by M.E.
        pulse-width range .000000005 sec to .001 sec continuous;
	neg-pulse-width range .000000005 sec to .001 sec continuous;
	pos-pulse-width range .000000005 sec to .001 sec continuous;
        period range .000000005 sec to 1000 sec continuous;
        voltage-p range .1 v to 100 v continuous;
        prf range .001 hz to 200000000 hz continuous;
      }

      begin FNC=4;  *risetime
        sensor (rise-time) pulsed dc;
      end;

      begin FNC=5;  *falltime
        sensor (fall-time) pulsed dc;
      end;
  
      begin FNC=6;  *pulsewidth
        sensor (pulse-width) pulsed dc ;  
      end;

      begin FNC=7;  *period
        sensor (period) pulsed dc;
      end;

      begin FNC=8;  *prf
        sensor (prf) pulsed dc;
      end;

      begin FNC=9;  *pulsewidth
        sensor (neg-pulse-width) pulsed dc ;  
      end;

      begin FNC=10;  *pulsewidth
        sensor (pos-pulse-width) pulsed dc ;  
      end;
    end; *pulsed dc
    
    begin; *count (totalize)
      sensor (count) events;
      control
      {
        voltage range .1 v to 100 v continuous;
        time range 0 sec to 5 sec continuous;
        count range 0 times to 999999999999 times continuous;
        pos-slope,neg-slope;
        trig range -102 v to 102 v continuous;
      }

      ** begin FNC=11;  *count (totalize)
      ** end;

      begin FNC=12;  *count (totalize) 1 by 2
        cnx gate-in CT-In2;
      end;
    end; *count (totalize)

    begin; *events for time interval A
      event monitor (voltage) pulsed dc;
      cnx event-out CT-EvA;
      * cnx event-out cntr-event-port  ;

      control
      {
        event-out;
        voltage range .1 v to 100 v continuous;
        prf range 0 hz to 200000000 hz continuous;
        settle-time .001 sec to 99.999 sec continuous;
      }

      begin FNC=13;  *events for interval A pos slope
        cnx hi CT-In1;
        control
        {
          event-slope pos;
        }
      end;

      begin FNC=14;  *time interval A neg slope
      cnx hi CT-In1;
        control
        {
          event-slope neg;
        }
      end;
    end; *events for time interval A
  end; * channel A

  begin using cntr-ch2;
    cnx via CT-In2;
    cnx hi CT-In2;

    cnx event-gate-from CT-EvArm;
