  begin; ** begin sources

    cnx via ATMOSPHERE;

    begin FNC = 101; ** infrared source 
      source infrared;
      control
       {
         max-time range 0.0 sec to 380.0 sec;
         differential-temp range -15.0 degc to 35.0 degc by 0.001 degc;
         diff-temp-error range 0.005 degc to 5.0 degc by 0.001 degc;
         target-type tets-opnapr, tets-4bar33, tets-4bar62, tets-4bar12, 
                     tets-4bar40, tets-rectangle, tets-diagln,
       	             tets-brsght, tets-pinmtx, tets-piesector;
       }
    end; 

    begin FNC = 102; ** visible light source 
      source light;
      control
       {
         max-time range 0.0 sec to 180.0 sec;
         radiance range 20.0 uw/cm2/sr to 20000.0 uw/cm2/sr by 1.0 uw/cm2/sr;
         angle-rate range 0.0 rads/sec to 3.0 rads/sec by 0.5 rads/sec;
         target-type tets-opnapr, tets-spnpinhl, tets-tgtgrp07, tets-tgtgrp12, 
                     tets-tgtgrp3, tets-etched, tets-brsght, tets-pinmtx,  
            	     tets-piesector, tets-gryscl10;
       }
    end; 

    begin FNC = 103; ** laser target return source 
      source laser target return;
      control
       {
         max-time range 0.0 sec to 360.0 sec;
         wave-length 1.064 um, 1.54 um, 1.57 um;
         target-range range 15.0 m to 40000.0 m;
         last-pulse-range range 19.5 m to 40000.0 m;
         trigger-mode lasr, auto;
         period range 50.0 msec to 125.0 msec continuous;
         pulse-width range 20.0e-9 sec to 150.0e-9 sec continuous;
         power-dens range 5.0E-10 w/cm2 to 5.0E-6 w/cm2;
       }
    end; 

    begin FNC = 104; ** Modulated (Chopped Light) Source
      source modulated signal;
      control
       {
         max-time range 0.0 sec to 60.0 sec;
         position 0, 1;
         freq range -1 hz to 10000 hz by 1 hz;
       }
    end; 

    begin FNC = 105; ** ALTA IR Boresight Source
      source multi-sensor-infrared;
      control
       {
         settle-time 0.0 sec to 600.0 sec;
         target-type tets-irbs00, tets-irbs01, tets-irbs02, tets-irbs03, tets-irbs04,
                     tets-irbs05, tets-irbs06, tets-irbs07, tets-irbs08, tets-irbs09,
                     tets-irbs10;
       }
    end; 

    begin FNC = 106; ** ALTA Visible (LED) Boresight Source
      source multi-sensor-light;
      control
       {
         max-time range 0.0 sec to 10.0 sec;
         target-type tets-tvbs01, tets-tvbs02, tets-tvbs03, tets-tvbs04, tets-tvbs05,
                     tets-tvbs06, tets-tvbs07, tets-tvbs08, tets-tvbs09, tets-tvbs10,
                     tets-tvbs11, tets-tvbs12, tets-tvbs13, tets-tvbs14, tets-tvbs15;
       }
    end; 

    begin FNC = 107; ** Puts ALTA in AUTOCOLLIMATION mode with camera controls
      source laser;
      control
       {
         max-time range 0.0 sec to 60.0 sec;
	 *** ALTA Laser Controls ***
         wave-length 1.064 um, 1.54 um, 1.57 um;
         pulse-width 150.0e-9 sec, 2.4e-6 sec, 38.4e-6 sec, 614.4e-6 sec;
         pulse-atten range 0 to 4095 by 1;
	 *** ALTA NIR Camera Controls ***
         delay range 0.0 sec to 6.553 sec by .000002 sec;
         sample-time range 17.9e-6 sec to 0.0333 sec;
         trigger-mode int, ext, lasr;
       }
      end;

  end;   *** end of sources 

