**************************************************************************
*****   RESERVED Name Macros to Perform IPC Initialization/Termination
**************************************************************************
def, mac, URGENTQUIT();
{
   Display("Entered URGENTQUIT\n");
   Stop();
   STATUS;
   enable 8 URGENTQUIT;
}
**************************************************************************
**************************************************************************
def, mac, RESETALL();
{
   SETDEVICE(<SYS_RESET>);
   TalkToBusDevice("FNC DCS VOLT :CH0\r\n");
}
**************************************************************************
**************************************************************************
def, mac, TERMINATE();
{
   disable 8;
   SETDEVICE(<SYS_TERM>);
   TalkToBusDevice("FNC DCS VOLT :CH0\r\n");
   STATUS;
}
**************************************************************************
**************************************************************************
def, mac, INITIALIZE();
   txt  buf 80;
{
   enable 8 URGENTQUIT;
   SETDEVICE(<SYS_INIT>);
   TalkToBusDevice("FNC DCS VOLT :CH0\r\n");
   STATUS;
}
